<script>
    document.addEventListener("DOMContentLoaded", function () {
        setTimeout(function () {
            var $TableHeight = $(".ui-jqgrid-bdiv").find("tbody").height();
            if ($TableHeight > 278) {
                $(".ui-jqgrid").find(".ui-jqgrid-bdiv").css("overflow", "auto");
                $(".m-table__responsive").find(".ui-jqgrid-htable").css("padding-right", "17px");
            }
            else {
                $(".m-table__responsive").find(".ui-jqgrid-htable").css("padding-right", "0px");



            }
        }, 200)
    });
    $(document).ready(function () {
        $('#ProjectId').select2({

            templateResult: function (data) {

                if (!data.id) {
                    // If no option is selected, display the default content
                    return data.text;
                }



                var option = $(data.element);
                var bgColor = option.data('bgcolor') || 'block';
                var text = option.text();
                // let classdef = bgColor == "#47d447" ? 'app' :'Approvedproject'


                // Set the background color for the option in the dropdown menu
                return $('<span style="color: ' + bgColor + ';  font-weight: bold;">' + text + '</span>');
            }
        });
    });
</script>
