@model Ideation.Models.PdfData
@{
    Layout = null;
}
<html>
<head>

    <link href="https://leap.himalayawellness.com/Logo_Font/css/PDFStyle.css" rel="stylesheet" />

    <style>

        .him_logo {
            float: right;
            width: 100%;
            text-align: right
        }


        .report-container td {
            font-family: 'rotis_semisans_stdregular';
            font-size: 14px !important;
            vertical-align: top;
        }

        /* .p {
                                    font-family: 'rotis_semisans_stdregular';
                                    font-size: 16px;
                                    vertical-align: top;
                                }*/

        .report-container b, .report-container strong {
            font-family: 'rotis_semisans_stdextrabold';
            font-weight: 700;
        }

        .page-break {
            page-break-before: always;
        }

        .image_td img {
            min-width: auto;
            max-width: 650px !important;
        }

        .image_td table {
            min-width: auto;
            max-width: 650px !important;
        }

            .image_td table td img {
                min-width: 100px !important;
                max-width: 100px !important;
            }
            .image_td table td:nth-child(1) {
                width: 100px;
            }

            .image_td table td:nth-child(2) {
                width: 100px;
            }

            .image_td table td:nth-child(3) {
                width: 100px;
            }

            .image_td table td:nth-child(4) {
                width: 100px;
            }

            .image_td table td:nth-child(5) {
                width: 100px;
            }

            .image_td table td:nth-child(6) {
                width: 100px;
            }

        .text_table img {
            width: 80% !important;
        }

        .text_table table {
            width: 98% !important;
        }

        .image_td table {
            max-width: 650px !important;
        }

            .image_td table td {
                white-space: break-spaces !important;
            }

        table td {
        }

        table th {
            word-wrap: anywhere;
        }

        thead.report-header {
            display: table-header-group;
        }

        tfoot.report-footer {
            display: table-footer-group;
        }

        table.report-container tr td, table.report-container tr th {
            page-break-inside: avoid;
            word-wrap: anywhere;
            margin: 4px 0 4px 0;
        }

        .Data_size span {
            font-family: 'rotis_semisans_stdregular';
            font-size: 12px;
            vertical-align: top;
        }
    </style>

</head>

<body style="width:100%; margin: 0px">
    <table class="report-container" style="width:100%;page-break-after:auto;overflow-wrap: break-word;page-break-inside:auto;">
        <thead class="report-header">
            <tr>
                <th>
                    <table style="width:100%;border: 1px solid #000;border-collapse: collapse;page-break-after:auto;display:block;overflow-wrap: break-word;">
                        <tr>
                            <td colspan="4" style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <strong>PROJECT BRIEF : @ViewBag.ProjectType - @ViewBag.ProjectId &nbsp;&nbsp; </strong>
                            </td>
                            <td rowspan="2" style="width:20%;word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <img src="https://leap.himalayawellness.com/Logo_Font/img/logohmnew.png" style="height:60px;" />
                            </td>
                        </tr>
                        <tr>
                            <td colspan="4" style="width:80%;word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong>Project Name :</strong> </label><label>@Model.ProjectName</label>
                            </td>
                           @* <td colspan="1" style="width:20%;word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong>Date :</strong> </label><label>@Model.Date</label>
                            </td>*@
                        </tr>
                        <tr>
                            <td style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong> Hub : </strong></label> &nbsp;<label>@Model.Hub</label>
                            </td>
                            <td style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label> <strong>Division :</strong> </label> &nbsp;<label>@Model.Division</label>
                            </td>
                            <td style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong> Category : </strong></label> &nbsp;<label>@Model.Category</label>
                            </td>
                            <td style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong> Initiated By : </strong></label> &nbsp;<label>@Model.InitiatedBy</label>
                            </td>
                            <td style="width:20%;word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong>Initiated Date : </strong></label> <br /><label>@Model.InitiatedDate</label>
                            </td>
                        </tr>
                    </table>

                </th>
            </tr>

        </thead>
        <tfoot class="report-footer">
            <tr>
                <td class="report-footer-cell">
                    <div class="footer-info">
                    </div>
                </td>
            </tr>
        </tfoot>

        <tbody class="report-content">

            <tr>
                <td class="report-content-cell">



                    <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;display:block;overflow-wrap: break-word;">


                        <tbody>
                            <tr>
                                <td class="image_td" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;overflow-wrap: break-word;word-break:break-word; page-break-inside: avoid;">
                                    <span style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;font-weight: 700;padding: 5px 5px 5px 5px;font-size: 16px"><b>Business Objective </b></span> <br />
                                    @Html.Raw(Model.BusinessObjective)
                                </td>
                            </tr>

                        </tbody>
                    </table>


                </td>
            </tr>
            <tr>
                <td>


                    @if (ViewBag.Status == "3" || ViewBag.Status == "4" || ViewBag.Status == "5"|| ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")
                    {
                        @if (Model.ProjectDetailsHgmlRemark != null)
                        {
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                                <tbody>
                                    <tr>
                                        <td scope="col" style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 5px;width:25%;font-size: 16px;font-weight: 700;"><b>HGML Remarks</b></td>
                                        <td scope="col" style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Model.ProjectDetailsHgmlRemark</td>
                                    </tr>
                                </tbody>
                            </table>
                        }

                    }



                    @if (ViewBag.Status == "6" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "12")
                    {
                        @if (Model.ProjectDetailsPmdRemarks != null)
                        {
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                                <tbody>
                                    <tr>
                                        <td scope="col" style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 5px;width:25%;font-size: 16px;font-weight: 700;"><b>Fine Screening Remarks</b></td>
                                        <td scope="col" style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Model.ProjectDetailsPmdRemarks</td>
                                    </tr>
                                </tbody>
                            </table>
                        }
                    }



                </td>
            </tr>
            @*    Project Details HUB remarks
            *@
            <tr>
                <td>

                    @if (ViewBag.Status == "4" || ViewBag.Status == "5"|| ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")
                    {
                        @if (ViewBag.IssenttoHUB == "YES")
                        {

                            <div>
                                <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;font-size: 16px">HUB Remarks </p>
                            </div>
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">
                                <thead>
                                    <tr>
                                        <td style="font-family:rotis_semisans_stdextrabold;word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;width: 20%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>HUB Name<b></b></td>
                                        <td style="font-family:rotis_semisans_stdextrabold;word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;width: 40%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>Remarks</b> </td>
                                    </tr>

                                </thead>
                                <tbody>
                                    @if (Model.ProjectDetailsHubRemarksList != null)
                                    {
                                        foreach (var item in Model.ProjectDetailsHubRemarksList)
                                        {

                                            <tr>
                                                <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.Hub)</td>
                                                <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.Remarks)</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        }
                    }
                </td>
            </tr>

            <tr>
                <td>

                    <div>
                        <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;font-size: 16px">Product Positioning </p>
                    </div>
                    <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                        <tbody>

                            <tr>
                                <td scope="col" style="font-family:rotis_semisans_stdextrabold;border: 1px solid black;padding: 5px 0px 5px 5px;width:25%;font-size: 16px;font-weight: 700;">Target Consumer</td>
                                <td scope="col" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere; page-break-inside: avoid;overflow-wrap: break-word;">@Html.Raw(Model.TargetConsumer)</td>
                            </tr>

                        </tbody>
                    </table>
                </td>
            </tr>

            <tr>
                <td>

                    <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">


                        <tbody>
                            <tr>
                                <td scope="col" class="image_td" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere; page-break-inside: avoid;overflow-wrap: break-word;">
                                    <span style="font-weight: 700;padding: 5px 5px 5px 5px;font-size: 16px"><b>Competitive Offerings  </b></span>
                                    @Html.Raw(Model.CompetitiveOfferings)
                                </td>
                            </tr>

                        </tbody>
                    </table>

                </td>
            </tr>

            <tr>
                <td>

                    <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">
                        <tbody>
                            <tr>
                                <td scope="col" class="image_td" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;">
                                    <span style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;font-size: 16px"> Unmet Need </span>
                                    @Html.Raw(Model.UnmetNeed)
                                </td>
                            </tr>

                        </tbody>
                    </table>
                </td>
            </tr>

            <tr>
                <td>
                    <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">
                        <thead>
                            <tr>
                                <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;width: 20%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>Product</b></td>
                                <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;width: 40%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>Expected Features </b></td>
                                <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;width: 40%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>Expected Benefits </b></td>
                                <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;width: 20%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>SKU </b></td>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model.ProductPositioning != null)
                            {
                                foreach (var item in Model.ProductPositioning)
                                {

                                    <tr>
                                        <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;"><p>@Html.Raw(item.Product)</p></td>
                                        <td class="image_td" style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.ExpectedFeatures)</td>
                                        <td class="image_td" style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.ExpectedBenefits)</td>
                                        <td class="image_td" style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.SKU)</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>

                </td>
            </tr>

            <tr>
                <td>

                    @if (ViewBag.Status == "3" || ViewBag.Status == "4" || ViewBag.Status == "5" ||ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")
                    {
                        @if (Model.ProductPositioningHgmlRemark != null)
                        {
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                                <tbody>
                                    <tr>
                                        <td scope="col" style="word-wrap: anywhere; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 5px;width:25%;font-size: 16px;font-weight: 700;"><b>HGML Remarks</b></td>
                                        <td scope="col" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;">@Model.ProductPositioningHgmlRemark</td>
                                    </tr>
                                </tbody>
                            </table>
                        }
                    }

                    @if (ViewBag.Status == "6" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "12")
                    {
                        @if (Model.ProductPositioningPmdRemarks != null)
                        {
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                                <tbody>
                                    <tr>
                                        <td scope="col" style="border: 1px solid black;padding: 5px 0px 5px 5px;width:25%;font-size: 16px;font-weight: 700;"><b>Fine Screening Remarks</b></td>
                                        <td scope="col" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;">@Model.ProductPositioningPmdRemarks</td>
                                    </tr>
                                </tbody>
                            </table>
                        }
                    }

                </td>
            </tr>
            <tr>
                <td>
                    @*   Product Posiioning HUB remarks*@


                    @if (ViewBag.Status == "4" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")
                    {

                        @if (ViewBag.IssenttoHUB == "YES")
                        {

                            <div>
                                <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;font-size: 16px"><b>HUB Remarks </b></p>
                            </div>
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                                <tbody>
                                    @if (Model.ProductPositioningHubRemarksList != null)
                                    {
                                        foreach (var item in Model.ProductPositioningHubRemarksList)
                                        {

                                            <tr>
                                                <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;"><b>HUB Name </b></td>
                                                <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.Hub) </td>
                                            </tr>
                                            <tr>
                                                <td class="image_td" colspan="2" style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 5px 5px 5px;">
                                                    <span style="font-weight: 700;padding: 0px 5px 5px 10px;"><b>Remarks </b></span>
                                                    @Html.Raw(item.Remarks)
                                                </td>
                                            </tr>

                                        }
                                    }
                                </tbody>
                            </table>
                        }
                    }



                </td>
            </tr>

            <tr>
                <td>

                    <div>
                        <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;font-size: 16px"> Product Profile</p>
                    </div>
                    <div>
                        <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;font-size: 16px"> Formulation Profile</p>
                    </div>

                    @if (Model.ProductProfile != null)
                    {


                        foreach (var item in Model.ProductProfile)
                        {
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                                <tbody>
                                    <tr>

                                        <td colspan="4" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Product :</b> @item.Product</td>
                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Benchmark Products :</b><br />@item.BenchmarkProducts</td>

                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Desired Ingredients :</b><br />@item.DesiredIngredients</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Indication / Conditions :</b><br />@item.IndicationOrConditions</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Dosage Form :</b><br />@item.DosageForm</td>



                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Nice to have claims :</b><br />@item.NiceToHaveClaims</td>

                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Must have claims :</b><br />@item.MustHaveClaims</td>
                                        <td colspan="2" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Desired Product Characteristics :</b><br />@item.DesiredProductCharacteristics</td>

                                    </tr>
                                </tbody>
                            </table>
                            @if (Model.PackagingProfileFieldRemarks != null)
                            {
                                foreach (var values in Model.PackagingProfileFieldRemarks)
                                {
                                    @if (item.Product == values.Product && values.SKU=="")
                                    {
                                        foreach (var id in Model.packagingMasterList)
                                        {
                                            @if (id.PackageId == values.PackageId)
                                            {

                                                <table width="100%" style="border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">
                                                    <tbody>
                                                        <tr>
                                                            <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>@Html.Raw(id.PackageName) Remarks by HGML:</b><br /> @Html.Raw(values.Remarks)</td>
                                                        </tr>
                                                    </tbody>
                                                </table>

                                            }

                                        }

                                    }
                                }

                            }
                            <br />

                        }
                    }


                </td>
            </tr>
            <tr>
                <td>

                    @{
                        int NoofImages = 0;
                    }
                    @if (Model.ProductProfile != null)
                    {
                        foreach (var item in Model.ProductProfile)
                        {
                            if (item.Imagecontent != null)
                            {
                                NoofImages++;
                            }
                        }
                    }


                    @if (NoofImages > 0)
                    {

                        <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                            <tbody>
                                <tr>
                                    <td scope="col" style="border: none;padding: 5px 0px 5px 5px;font-size: 14px;font-weight: 600;"><b>Benchmark Products Image(s)</b></td>
                                </tr>
                                @if (Model.ProductProfile != null)
                                {
                                    foreach (var item in Model.ProductProfile)
                                    {
                                        if (item.Imagecontent != null)
                                        {
                                    // var imagecontent = item.Imagecontent.Split(',');
                                        //foreach (var image in imagecontent)
                                            // {
                                            <tr>
                                                <td scope="col" style="border: none;padding: 5px 0px 5px 5px;font-size: 14px;font-weight: 600;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><img src="@item.Imagecontent" width="400" height="300" /></td>
                                            </tr>
                                            // }
                                        }
                                    }
                                }
                            </tbody>
                        </table>
                    }
                </td>
            </tr>
            <tr>
                <td>

                    @if (ViewBag.Status == "3" || ViewBag.Status == "4" || ViewBag.Status == "5" ||ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")
                    {
                        @if (Model.FormulationProfileHgmlRemark != null)
                        {
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

