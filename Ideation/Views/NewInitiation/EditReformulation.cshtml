                                <span id="nameAlreadyExists_Err" style="color:red;display:none" class="removeError">Product already exists. Please add another product</span>
                            </div>
                        </div><div class="col-md-3">
                            <div class="form-group">
                                <label class="control-label">New Brand Name (if applicable) </label>
                                @Html.TextBoxFor(model => model.NewBrandName, new { @id = "ProductDescriptionNewBrandName", @class = "form-control ProductDesctipitonEmpty NoEdit ViewMode" })
                            </div>
                        </div>
