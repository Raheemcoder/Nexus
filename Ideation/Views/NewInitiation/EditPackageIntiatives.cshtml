                    <label class="col-sm-2 col-form-label">HGML Remarks</label>
                    <div class="col-sm-10 pd">
                        @Html.TextAreaFor(model=>model.PackageProductDescriptionHGMLReamrks, new{@id="editor", name="editor2",cols="30",@class="form-control form-control-sm PackProductDescriptionHGMLReamrks"})
