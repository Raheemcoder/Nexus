            @if (Model.ID == "1" || Model.ID == "8" || Model.ID == "9" || Model.ID == "11")
            {
                <div class="row mb-3">
                    <label class="col-sm-2 col-form-label">Project Name <span class="color-red">*</span></label>
                    <div class="col-sm-10 pd">
                        @Html.TextBoxFor(m=>m.ProjectName, new{ id="ProductDescription_ProjectName",@class="form-control form-select-sm"})
                        @Html.ValidationMessageFor(model => model.ProjectName,"", new { @class = "text-danger" })
                        <span style="color:red" id="Error_in_ProjectName" hidden>Please enter Project Name</span>
                    </div>
                </div>
            }
            else
            {
                <div class="row mb-3">
                    <label class="col-sm-2 col-form-label">Project Name <span class="color-red">*</span></label>
                    <div class="col-sm-10 pd">
                        @Html.TextBoxFor(m=>m.ProjectName, new{ id="ProductDescription_ProjectName",@class="form-control form-select-sm",@readonly="readonly"})
                    </div>
                </div>

            }

