            @if (Model.Status == "View")
            {
                <h3 class="cen_txt" id="statusHeader"></h3>
            }
            @if (Model.ID == "1")
            {
                <h3 class="cen_txt d-grid">Packaging Initiative-Draft <span class="sub_head">(<strong>Note:</strong> The Project Brief information will be auto saved for every 5 minutes . Please ensure that Project Name is entered to Auto save the data.)</span></h3>
            }
            @if (Model.ID == "2")
            {
                <h3 class="cen_txt">Packaging Initiative-HGML Review</h3>
            }
            @if (Model.ID == "3")
            {
                <h3 class="cen_txt">Packaging Initiative-HUB Review</h3>
            }
            @if (Model.ID == "4")
            {
                <h3 class="cen_txt">Packaging Initiative-HGML Approve</h3>
            }

            @if (Model.ID == "5")
            {
                <h3 class="cen_txt">Packaging Initiative-Fine Screening Review</h3>
            }
            @if (Model.ID == "16")
            {
                <h3 class="cen_txt">Packaging Initiative-Under Exploration</h3>
            }
            @if (Model.ID == "6")
            {
                <h3 class="cen_txt">Packaging Initiative-Accepted</h3>
            }
            @if (Model.ID == "7")
            {
                <h3 class="cen_txt">Packaging Initiative-Rejected</h3>
            }
            @if (Model.ID == "8")
            {
                <h3 class="cen_txt d-grid">Packaging Initiative-Sent Back To Initiator <span class="sub_head">(<strong>Note:</strong> The Project Brief information will be auto saved for every 5 minutes . Please ensure that Project Name is entered to Auto save the data.)</span></h3>

            }
            @if (Model.ID == "9")
            {
                <h3 class="cen_txt">Packaging Initiative-My approval Pending</h3>
            }
            @if (Model.ID == "11")
            {
                <h3 class="cen_txt d-grid">Packaging Initiative-Brief Demoted to Initiator  <span class="sub_head">(<strong>Note:</strong> The Project Brief information will be auto saved for every 5 minutes . Please ensure that Project Name is entered to Auto save the data.)</span></h3>
            }
            @if (Model.ID == "12")
            {
                <h3 class="cen_txt">Packaging Initiative-Updated</h3>
            }
            @if (Model.ID == "13" || Model.ID == "14")
            {
                <h3 class="cen_txt">Packaging Initiative-Brief Demoted to HGML</h3>
            }

