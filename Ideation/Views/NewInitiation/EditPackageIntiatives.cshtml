                                <select class="form-select-sm addCategoryOption data-singleselect" id="Package_Category" name="Category" disabled>
                                    <option value="">--Select--</option>
                                    @Html.HiddenFor(model => model.Category, new { @class="form-control form-control-sm", @id="Category"} )
                                </select>
                            </td>
                            @if (Model.ID == "3" || Model.Status == "View")
                            {

                                <td id="PackageInitiative_IsHubApprove" class="forhubonly header_table_font">
                                    @Html.DropDownListFor(model => model.IsHubApproved, new List<SelectListItem>(){

                        new SelectListItem() { Text= "Yes", Value = "Yes" },
                        new SelectListItem() { Text= "No", Value = "No" }
                        },"--Select--",new {@id="PackHubApprove", @class="form-select-sm"})
