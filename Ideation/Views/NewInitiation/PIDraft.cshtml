@model Ideation.Models.PdfData
@{
    Layout = null;
}
<html>
<head>

    <link href="https://leap.himalayawellness.com/Logo_Font/css/PDFStyle.css" rel="stylesheet" />
    <style>

        .him_logo {
            float: right;
            width: 100%;
            text-align: right
        }

        .report-container td {
            font-family: 'rotis_semisans_stdregular';
            font-size: 14px !important;
            vertical-align: top;
        }

        .report-container p {
            font-family: 'rotis_semisans_stdregular';
            font-size: 16px;
            vertical-align: top;
        }

        .report-container b, .report-container strong {
            font-family: 'rotis_semisans_stdextrabold';
            font-weight: 700;
        }

        .page-break {
            page-break-before: always;
        }

        .image_td img {
            min-width: auto;
            max-width: 650px !important;
        }

        .image_td table {
            width: 98% !important;
        }

            .image_td table td img {
                min-width: 100px !important;
                max-width: 100px !important;
            }
            .image_td table td:nth-child(1) {
                width: 100px;
            }

            .image_td table td:nth-child(2) {
                width: 100px;
            }

            .image_td table td:nth-child(3) {
                width: 100px;
            }

            .image_td table td:nth-child(4) {
                width: 100px;
            }

            .image_td table td:nth-child(5) {
                width: 100px;
            }

            .image_td table td:nth-child(6) {
                width: 100px;
            }

        .image_td table {
            width: 96% !important;
        }

            .image_td table td {
                white-space: break-spaces !important;
            }
        /*  .table .td_pad .td {
                                                                                                                                                            padding: 0px 0px 0px 0px
                                                                                                                                                        }*/

        /* table.report-container {
                                                                                                                                                            page-break-after: always;
                                                                                                                                                            tabel-layout:fixed;
                                                                                                                                                        }*/

        table td {
        }

        table th {
            word-wrap: anywhere;
        }

        thead.report-header {
            display: table-header-group;
        }

        tfoot.report-footer {
            display: table-footer-group;
        }

        table.report-container tr td, table.report-container tr th {
            page-break-inside: avoid;
            word-wrap: anywhere;
            margin: 4px 0 4px 0;
        }

        .Data_size span{
            font-family: 'rotis_semisans_stdregular';
            font-size: 12px;
            vertical-align: top;
        }
    </style>
</head>

<body style="width:850px; margin: 0px">

    <table class="report-container" style="width:100%;page-break-after:auto;overflow-wrap: break-word;page-break-inside:auto;">
        <thead class="report-header">
            <tr>
                <th>
                    <table style="width:100%;border: 1px solid #000;border-collapse: collapse;page-break-after:auto;display:block;overflow-wrap: break-word;">
                        <tr>
                            <td colspan="4" style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <strong>PROJECT BRIEF : @ViewBag.ProjectType - @ViewBag.ProjectId &nbsp;&nbsp; </strong>
                            </td>
                            <td rowspan="2" style="width:20%;word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <img src="https://leap.himalayawellness.com/Logo_Font/img/logohmnew.png" style="height:60px;" />


                            </td>
                        </tr>
                        <tr>
                            <td colspan="4" style="width:80%;word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong>Project Name : </strong> </label><label>@Model.ProjectName</label>
                            </td>
                            @* <td colspan="1" style="width:20%;word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                            <label><strong>Date : </strong> </label><label>@Model.Date</label>
                            </td>*@
                        </tr>
                        <tr>
                            <td style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong> Hub : </strong></label> &nbsp;<label>@Model.Hub</label>
                            </td>
                            <td style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong> Division : </strong></label> &nbsp;<label>@Model.Division</label>
                            </td>
                            <td style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong> Category : </strong></label> &nbsp;<label>@Model.Category</label>
                            </td>
                            <td style="word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong> Initiated By : </strong></label> &nbsp;<label>@Model.InitiatedBy</label>
                            </td>
                            <td style="width:20%;word-wrap: anywhere; border: 1px solid black;padding: 5px 0px 5px 5px;text-align: center; page-break-inside: avoid;overflow-wrap: break-word;">
                                <label><strong>Initiated Date : </strong></label> <br /><label>@Model.InitiatedDate</label>
                            </td>
                        </tr>
                    </table>

                </th>
            </tr>

        </thead>
        <tfoot class="report-footer">
            <tr>
                <td class="report-footer-cell">
                    <div class="footer-info">
                    </div>
                </td>
            </tr>
        </tfoot>
        <tbody class="report-content">
            <tr>
                <td class="report-content-cell">
                    <div>
                        <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px">Product Description </p>
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">
                        <thead>

                            <tr>
                                <td style="word-wrap: anywhere;overflow-wrap: break-word;page-break-inside: avoid; border: 1px solid black;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>Product Name</b></td>
                                <td style="word-wrap: anywhere;overflow-wrap: break-word;page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>New Brand Name (if applicable)</b></td>
                                <td style="word-wrap: anywhere;overflow-wrap: break-word;page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>SKU</b></td>

                            </tr>

                        </thead>
                        <tbody>
                            @if (Model.ProducDescription != null)
                            {

                                foreach (var item in Model.ProducDescription)
                                {
                                    <tr>
                                        <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@item.ProductName</td>
                                        <td style="word-wrap: anywhere ;overflow-wrap: break-word;page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@item.NewBrandName</td>
                                        <td style="word-wrap: anywhere;overflow-wrap: break-word; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@item.SKU</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td>
                    <br />

                    @if (ViewBag.Status == "3" || ViewBag.Status == "4" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")
                    {
                        <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                            <tbody>

                                @if (Model.ProductPositioningHgmlRemark != null)
                                {

                                    <tr>
                                        <td scope="col" style="word-wrap: anywhere; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 5px;width:25%;font-size: 16px;font-weight: 700;"><b>HGML Remarks</b></td>
                                        <td scope="col" style="word-wrap: anywhere; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(Model.ProductPositioningHgmlRemark)</td>
                                    </tr>
                                }

                                @if (ViewBag.Status == "6" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "12")
                                {
                                    @if (Model.PackageProductDescriptionPMDRemarks != null)
                                    {
                                        <tr>
                                            <td scope="col" style="word-wrap: anywhere; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 5px;width:25%;font-size: 16px;font-weight: 700;"><b>Fine Screening Remarks</b></td>
                                            <td scope="col" style="word-wrap: anywhere; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(Model.PackageProductDescriptionPMDRemarks)</td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    }

                </td>
            </tr>
            <tr>
                <td>
                    @if (ViewBag.Status == "4" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")
                    {
                        <br />
                        @if (ViewBag.IssenttoHUB == "YES" && Model.HubProdctDescriptionRemarks?.Count() != 0)
                        {
                            <div>
                                <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;">HUB Remarks </p>
                            </div>
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">
                                <thead>
                                    <tr>
                                        <td style="word-wrap: anywhere; page-break-inside: avoid;border: 1px solid black;width: 20%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>HUB Name</b></td>
                                        <td style="word-wrap: anywhere; page-break-inside: avoid;border: 1px solid black;width: 40%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>Remarks</b></td>
                                    </tr>

                                </thead>
                                <tbody>
                                    @if (Model.HubProdctDescriptionRemarks != null)
                                    {
                                        foreach (var item in Model.HubProdctDescriptionRemarks)
                                        {
                                            <tr>
                                                <td style="word-wrap: anywhere; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.HUBName)</td>
                                                <td style="word-wrap: anywhere; page-break-inside: avoid;border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.ProductDescription)</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        }
                    }
                </td>
            </tr>

            <tr>
                <td>




                    <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">


                        <tbody>
                            <tr>
                                <td style="margin-bottom:10px;padding-bottom:10px">
                                    <span style="font-weight: 700;font-size:16px;padding: 5px 5px 5px 5px;"><b>Project Details  </b></span>
                                </td>
                            </tr>
                            <tr>
                                <td style="margin-bottom:10px;padding-bottom:10px;padding-top:10px">
                                    <span style="font-size:14px;font-weight: 600;padding: 5px 5px 5px 5px;"><b>Business Rational (Reason for Proposed change)</b></span>
                                </td>
                            </tr>
                            <tr style="border: 1px solid #000;border-collapse: collapse;table-layout:fixed;display:block;overflow-wrap: break-word;">
                                <td scope="col" class="image_td" style="word-wrap: anywhere;border: 1px solid black;padding: 5px 0px 5px 10px;">

                                    @Html.Raw(Model.BusinessObjective)
                                </td>
                            </tr>

                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td>
                    <br />
                    @if (ViewBag.Status == "3" || ViewBag.Status == "4" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")
                    {
                        <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">
                            <tbody>
                                @if (Model.ProjectDetailsHgmlRemark != null)
                                {
                                    <tr>
                                        <td scope="col" style="border: 1px solid black;padding: 5px 0px 5px 5px;font-size: 16px;font-weight: 700;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>HGML Remarks</b></td>
                                        <td scope="col" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;">@Html.Raw(Model.ProjectDetailsHgmlRemark)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    @if (ViewBag.Status == "6" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "12")
                    {
                        @if (Model.PackProjectDetailsPMDRemarks != null)
                        {
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">
                                <tbody>
                                    <tr>
                                        <td scope="col" style="border: 1px solid black;padding: 5px 0px 5px 5px;font-size: 16px;font-weight: 700;"><b>Fine Screening Remarks</b></td>
                                        <td scope="col" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;">@Html.Raw(Model.PackProjectDetailsPMDRemarks)</td>
                                    </tr>
                                </tbody>
                            </table>
                        }
                    }



                </td>
            </tr>
            @if (ViewBag.Status == "4" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")

            {
                @if (ViewBag.IssenttoHUB == "YES")
                {

                    <tr>
                        <td>
                            <div>
                                <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;">HUB Remarks </p>
                            </div>
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">
                                <thead>
                                    <tr>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>HUB Name</b></td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>Remarks</b> </td>
                                    </tr>

                                </thead>
                                <tbody>
                                    @if (Model.HubProjectDetailsRemarks != null)
                                    {
                                        foreach (var item in Model.HubProjectDetailsRemarks)
                                        {

                                            <tr>
                                                <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;">@Html.Raw(item.HUBName)</td>
                                                <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;">@Html.Raw(item.PackProjectDetails)</td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </td>
                    </tr>
                }
            }

            <tr>
                <td>
                    <div>
                        <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;">Business Information </p>
                    </div>
                    @if (ViewBag.Status != "3")
                    {
                        @*<table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;overflow-wrap: break-word;">

                            <tr>
                                <td scope="col" style="border: 1px solid black;padding: 5px 0px 5px 5px;width:25%;font-size: 16px;font-weight: 700;"><b><b>Total Business value (USD)</b></b></td>
                                <td style="border: 1px solid black;width: 40%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:left">@Model.TotalBusinessValue</td>
                            </tr>

                        </table>*@

                    }
                    <br />
                    @if (Model.BusinessInformationdata != null)
                    {
                        foreach (var item in Model.BusinessInformationdata)
                        {
                            <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;">

                                <tbody>
                                    <tr>
                                        <td colspan="4" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;"><b>Product : </b> @Html.Raw(item.Product)</td>
                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b> SKU :</b><br /> @Html.Raw(item.SKU)</td>

                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b> Proposed launch date :</b><br /> @Html.Raw(item.ProposedLaunchDate)</td>

                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b> Proposed SP :</b><br /> @Html.Raw(item.ProposedSellingPrice)</td>

                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Expected COP :</b><br /> @Html.Raw(item.ProposedTP)</td>

                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Proposed MRP :</b><br /> @Html.Raw(item.ProposedMRP)</td>

                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Currency :</b><br /> @Html.Raw(item.Currency)</td>

                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Expected GP % :</b><br /> @Html.Raw(item.ExpectedGP)</td>

                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Business Value(Y2*PSP) :</b><br /> @Html.Raw(item.BusinessValue)</td>

                                    </tr>

                                    <tr>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>M1 : </b> @Html.Raw(item.M1Quantity)</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>M2 : </b> @Html.Raw(item.M2Quantity)</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>M3 : </b> @Html.Raw(item.M3Quantity)</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>M4 : </b> @Html.Raw(item.M4Quantity)</td>
                                    </tr>
                                     <tr>
                                 
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>M5 : </b> @Html.Raw(item.M5Quantity)</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>M6 : </b> @Html.Raw(item.M6Quantity)</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Y1 : </b> @Html.Raw(item.Y1Quantity)</td>
                                          <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Y2 : </b> @Html.Raw(item.Y2Quantity)</td>
                                    </tr>
                                    <tr>                                     
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Y3 : </b> @Html.Raw(item.Y3Quantity)</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>UOM : </b> @Html.Raw(item.UOM)</td>


                                    </tr>



                                </tbody>
                            </table>
                        }


                        <br />



                    }




                </td>
            </tr>
            <tr>
                <td>
                    @if (ViewBag.Status == "4" || ViewBag.Status == "5" || ViewBag.Status == "16" || ViewBag.Status == "6" || ViewBag.Status == "12" || ViewBag.Status == "14")
                    {
                        @if (ViewBag.IssenttoHUB == "YES")
                        {
                            <div>
                                <p style="font-family:rotis_semisans_stdextrabold;font-weight: 700;padding: 5px 5px 5px 5px;">Other HUB's Business Information</p>
                            </div>




                            @if (Model.PackageInitiativesBusinessInformation != null)
                            {
                                foreach (var item in Model.PackageInitiativesBusinessInformation)
                                {
                                    <table width="100%" style="border: 1px solid #000;border-collapse: collapse;page-break-after:auto;table-layout:fixed;display:block;">

                                        <tbody>
                                            <tr>
                                                <td colspan="4" style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;"><b>Product : </b> @Html.Raw(item.Product)</td>
                                            </tr>
                                            <tr>
                                                <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b> HUB :</b><br /> @Html.Raw(item.HubName)</td>
                                                <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b> SKU :</b><br /> @Html.Raw(item.SKU)</td>

                                                <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b> Proposed launch date :</b><br /> @Html.Raw(item.ProposeLaunchDate)</td>

                                                <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b> Proposed SP :</b><br /> @Html.Raw(item.ProposedSellingPrice)</td>


                                            </tr>
                                            <tr>
                                                <td style="border: 1px solid black;padding: 5px 0px 5px 10px;word-wrap: anywhere;page-break-inside: avoid;overflow-wrap: break-word;"><b>Expected COP :</b><br /> @Html.Raw(item.ProposedTP)</td>

