@model Ideation.Models.NPD;
@{
    ViewBag.Title = "EditProduction";
    Layout = "~/Views/Shared/_PBLayout.cshtml";
    var userName = Context.Session.GetString("UserName");
    var hub = Context.Session.GetString("Hub");
    Model.UserName = userName;

}
<link href="https://www.codehim.com/demo/bootstrap-multiselect-dropdown/dist/css/bootstrap-multiselect.css" rel="stylesheet" />
<script src="@Url.Content("https://www.codehim.com/demo/bootstrap-multiselect-dropdown/dist/js/bootstrap-multiselect.js")" asp-append-version="true"></script>
<script src="@Url.Content("https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js")" asp-append-version="true"></script>
<script src="@Url.Content("~/ckeditor/ckeditor.js")" asp-append-version="true"></script>
<script src="@Url.Content("~/js/jquery.validate.min.js")" asp-append-version="true"></script>
<script src="@Url.Content("~/js/jquery.validate.unobtrusive.min.js")" asp-append-version="true"></script>

<style>
    .btn-group > .btn {
        border-radius: 5px;
        border-color: #c6c6c6;
        height: 36px;
        text-align: left;
        font-size: 13px;
    }

    .multiselect-container.dropdown-menu {
        top: auto !important;
        bottom: 36px;
        max-height: 190px !important;
    }
</style>

<div class="card-box mb-0">
    @using (Html.BeginForm("EditProduction", "NewInitiation", FormMethod.Post, new { id = "Npd_Edit_Form_Submit", enctype = "multipart/form-data" }))
    {

        @Html.HiddenFor(model=>model.JsonFormNpdData)
        @Html.HiddenFor(model=>model.JsonFormNpdHgmlReviewData)
        @Html.HiddenFor(model=>model.JsonFormNpdHubReviewData)
        @Html.HiddenFor(model=>model.JsonFormNpdHgmlApproveData)
        @Html.HiddenFor(model=>model.JsonFormNpdPmdReviewData)

        @Html.HiddenFor(model=>model.UserName,new{@Value=@userName,@id="UserName"})

        @Html.HiddenFor(model => model.ProjectId,new{@id="ProjectId"})
        @Html.HiddenFor(model => model.StatusNameDb,new{@id="StatusNameDb"})
        @Html.HiddenFor(model => model.NpdHeaderTableData)
        @Html.HiddenFor(model => model.ProjectDetailsData)
        @Html.HiddenFor(model => model.ProductPositionigData)
        @Html.HiddenFor(model => model.FormulationProfileData)
        @Html.HiddenFor(model => model.PackagingProfileData)
        @Html.HiddenFor(model => model.BusinessInformationData)
        @Html.HiddenFor(model => model.SustainabilityData)
        @Html.HiddenFor(model => model.PmdData)
        @Html.HiddenFor(model => model.TargetCostData,new{@id="NpdTargetCostData"})
        @Html.HiddenFor(model => model.HgmlApproveSendBackHubRemarksData)

        @Html.HiddenFor(model => model.HgmlData)
        @Html.HiddenFor(model => model.Status,new{@id="NpdStatus"})
        @Html.HiddenFor(model=>model.ApprovalStatus)
        @Html.HiddenFor(model=>model.SendToHubRemarks)
        @Html.HiddenFor(model=>model.SendToPmdRemarks)
        @Html.HiddenFor(model=>model.SendBackToInitiatorRemarks)
        @Html.HiddenFor(model=>model.RejectRemarks)
        @Html.HiddenFor(model=>model.SendToHgmlRemarks)

        @Html.HiddenFor(model=>model.ConfirmationRemarks)

        @Html.HiddenFor(model => model.CurrentStatusName,new{@id="NpdCurrentStatusName"})

        @Html.HiddenFor(model => model.Icon,new{@id="IconName"})
        @Html.HiddenFor(model => model.IsEdit)

        @Html.HiddenFor(model=>model.ReceivedDate, new {@id="ReceivedDate"})
        @Html.HiddenFor(model=>model.SavedRemarks, new {@id="SavedRemarks"})
        @Html.HiddenFor(model=>model.DeletedRemarks, new {@id="DeletedRemarks"})

        @Html.HiddenFor(model => model.SupportingDocumentData)
        @Html.HiddenFor(model => model.DeletedSupportingdocument)

        @Html.HiddenFor(model=>model.PMdDueDate)

        <div class="tb_clr marbt">
            <h3 class="cen_txt" id="Page_Heading"></h3>
            <table id="NPD_Table" class="table table-bordered Npd_HeaderTable">
                <thead>
                    <tr class="mod_th">
                        <th scope="col">Hub</th>
                        <th scope="col">Division<span class="color-red">*</span></th>
                        <th scope="col">Category<span class="color-red">*</span></th>
                        <th class="NotInNpd NotInNpdHgmlReview NotInHgmlApprove NotInPmdReview NotInApproved" scope="col">HUB Approve<span class="color-red">*</span><br /><span id="Error_HubApprove" class="color-red" style="display:none">Please select HUB Approve</span></th>
                        <th scope="col">Initiated By</th>
                        <th scope="col">Initiated Date</th>
                        <th scope="col">Status</th>
                        <th scope="col">Remarks</th>
                        <th scope="col">Download</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="header_table_font" id="NPD_Hub">@hub</td>
                        <td class="header_table_font" id="NPD_Division">
                            @Html.DropDownListFor(model => model.Division,Model.DivisionList,"--Select--",new { @class = "form-select-sm", @id="Division"})
                            @Html.ValidationMessageFor(model => model.Division, "", new { @class = "text-danger" })
                        </td>
                        <td class="header_table_font" id="NPD_Category">
                            <select class="form-select-sm addCategoryOption data-singleselect" id="Npd_Category" name="Category">
                                <option value="">--Select--</option>
                                @Html.HiddenFor(model => model.Category, new { @class="form-control form-control-sm", @id="Category"} )
                            </select>
                            @Html.ValidationMessageFor(model => model.Category, "", new { @class = "text-danger" })
                            <span id="Message_Npd_Category" style="display:none"><br />Please select Division First</span>
                        </td>
                        <td class="header_table_font NotInNpd NotInNpdHgmlReview NotInHgmlApprove NotInPmdReview NotInApproved" id="Npd_HubApproveConfirmation">
                            @Html.DropDownListFor(model => model.HubApproveConfirmation, new List<SelectListItem>()
                        {
                        new SelectListItem() { Text= "--Select--", Value = "",Selected =true  },
                        new SelectListItem() { Text= "Yes", Value = "Yes" },
                        new SelectListItem() { Text= "No", Value = "No" }
                        },new {@id="HubApproveConfirmation", @class="form-select-sm"})
                        </td>
                        <td class="header_table_font" id="NPD_InitiatedBy"></td>
                        <td class="header_table_font" id="NPD_InitiatedDate"></td>
                        <td class="header_table_font" id="NPD_Status"></td>
                        <td class="rd1"><a href="@Url.Content("#")" data-bs-toggle="modal" onclick="onClickOfApprovalRemarks()"><i class="fa fa-info-circle" aria-hidden="true" title="Remarks"></i></a></td>
                        <td><a><i class="fa fa-download downloadPdf" aria-hidden="true" title="Download"></i></a></td>
                    </tr>
                </tbody>
            </table>
            <form class="bck_prd">
                <h6 class="pro_det">Project Details</h6>
                <div class="row mb-3">
                    <label class="col-sm-2 col-form-label">Project Name <span class="color-red">*</span></label>
                    <div class="col-sm-10 pd">
                        @Html.TextBoxFor(model => model.ProjectName, new { @class = "form-control form-control-sm", @id="Npd_ProjectName"})
                        @*                        @Html.ValidationMessageFor(model => model.ProjectName, "", new { @class = "text-danger" })
                    *@                        <span style="color:red; display:none" id="Error_Npd_ProjectName" class="Error_ProjectDetails">Please enter Project Name</span>
                    </div>
                </div>
                <div class="row mb-3">
                    <lable class="col-sm-2 col-form-label">Business Objective <span class="color-red">*</span></lable>
                    <div class="ckediter_tolbar col-sm-10">
                        @Html.TextAreaFor(model => model.BusinessObjective, new { @name="editor2", @class = "form-control", @id="Npd_BusinessObjective", @cols="30" })
                        @Html.ValidationMessageFor(model => model.BusinessObjective, "", new { @class = "text-danger" })
                        <span style="color:red; display:none" id="Error_Npd_BusinessObjective" class="Error_ProjectDetails">Please enter Business Objective </span>
                    </div>
                </div>

                <div class="add_grid_ NotInNpd NotInNpdHgmlReview NotInHubReview Project_Details_HubRemarks">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="">
