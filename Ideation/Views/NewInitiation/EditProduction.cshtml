                                <label class="col-form-label">Expected Features <span class="color-red">*</span></label>
                                <div class="ckediter_tolbar feature_benefits ">
                                    @Html.TextAreaFor(model => model.ProductPositioningExpectedFeatures, new { @name="editor4", @class = "form-control ProductPositioning", @id="PP_ExpectedFeatures", @cols="30" })
                                    <textarea class="form-control" id="expected_profile"></textarea>
                                    <span style="color:red; display:none" id="Error_PP_ExpectedFeatures" class="Error_ProductPositioning">Please enter Expected Features </span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">Expected Benefits <span class="color-red">*</span></label>
                                <div class="ckediter_tolbar feature_benefits">
                                    @Html.TextAreaFor(model => model.ProductPositioningExpectedBenefits, new { @name="editor4", @class = "form-control ProductPositioning", @id="PP_ExpectedBenefits", @cols="30" })
                                    <textarea class="form-control" id="benefits_profile"></textarea>
                                    <span style="color:red; display:none" id="Error_PP_ExpectedBenefits" class="Error_ProductPositioning">Please enter Expected Benefits </span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <lable class="col-form-label" style="position:relative; top:4px;">SKU <span class="color-red">*</span></lable>
                                <div class="col-sm-12 mt-3">
                                    @Html.TextBoxFor(model => model.ProductPositioningSku, new { @class = "form-control ProductPositioning", @id="PP_Sku", @style="border-color:#d1d1d1" })
                                    <span style="color:red; display:none" id="Error_PP_Sku" class="Error_ProductPositioning">Please enter SKU</span>
                                    <span style="font-size:10px">(Please enter the SKU values with comma separated (Ex: 50 ml , 100 ml))</span>
                                </div>
                            </div>
                        </div>
                        @* <div class="col-md-1 pl-0">
                    <button type="button" id="Add_ProductPositioning" class="btn-add margin_top115"> Add</button>
                    </div>*@
                        <div class="col-md-3">
                            <div class="float-left text-left mt-3 pt-3">
                                <button type="button" id="Add_ProductPositioning" class="btn-add mt-1"> Add</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="add_grid_">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="">
                                    <div class="m-table__main mt-2 mb-4">
                                        <div class="m-table__responsive -virtual-scroll">
                                            <table id="Product_Positioning" class="table table-bordered"></table>
                                            <div id="pager_worksheet"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="Product_Positioning_HubRemarks_Link">
                        <div class="pro_det1 NotInNpd NotInNpdHgmlReview NotInHubReview">
                            <h6 class="">HUB Remarks</h6>
                        </div>
                        <div class="row NotInNpd NotInNpdHgmlReview NotInHubReview">
                            <div class="col-md-12">
                                <div class="hub_align hub_fit" id="ProductPositioningHubRemarks_Link">

                                    @*Hub remarks link*@

                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row product_profile mb-3 NotInNpd">
                        <label class="col-sm-2 col-form-label">HGML Remarks</label>
                        <div class="col-sm-10 pd">
                            @Html.TextAreaFor(model=>model.ProductPositioningHGMLRemarks, new { @id="PP_HGML_Remarks", @name="editor2", @cols="30", @class="form-control form-control-sm" })
                        </div>
                    </div>
                    <div class="row mb-3 NotInNpd NotInNpdHgmlReview NotInHgmlApprove NotInPmdReview NotInApproved">
                        <lable class="col-sm-2 col-form-label">@hub Remarks<span class="color-red">*</span></lable>
                        <div class="ckediter_tolbar col-sm-10">
                            @Html.TextAreaFor(model=>model.ProductPositioningHubRemarks, new { @id="PP_HUB_Remarks", @name="editorhusa", @cols="30", @class="form-control" })
                            <span class="color-red" id="Error_Npd_Hub_Remarks"></span>
                            @*                            <span style="color:red;" id="Error_Npd_Hub_Remarks" class="Error_HubRemarks">Please enter Product Positioning HubRemarks</span>
