                                    <div class="m-table__responsive -virtual-scroll">
                                        <table id="Project_Details_Remarks" class="table table-bordered"></table>
                                        <div id="pager_worksheet1"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row mb-3 NotInNpd">
                    <label class="col-sm-2 col-form-label">HGML Remarks</label>
                    <div class="col-sm-10 pd">
                        @Html.TextAreaFor(model=>model.ProjectDetailsHGMLRemarks, new { @id="PD_HGML_Remarks", @name="editor2", @cols="30", @class="form-control form-control-sm" })
                    </div>
                </div>
                <div class="row mb-3 NotInNpd NotInNpdHgmlReview NotInHubReview NotInHgmlApprove">
