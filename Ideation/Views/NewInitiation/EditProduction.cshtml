                    <label class="col-sm-2 col-form-label">Fine Screening Remarks</label>
                    <div class="col-sm-10 pd">
                        @Html.TextAreaFor(model=>model.ProjectDetailsPmdRemarks, new { @id="PD_Pmd_Remarks", @name="editor2", @cols="30", @class="form-control form-control-sm" })

                    </div>
                </div>
                <div class="row mb-3 NotInNpd NotInNpdHgmlReview NotInHgmlApprove NotInPmdReview NotInApproved">
                    <label class="col-sm-2 col-form-label">@hub Remarks</label>
