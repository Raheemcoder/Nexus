@model Ideation.Models.NPD;
@{
    ViewBag.Title = "NewProduction";
    Layout = "~/Views/Shared/_PBLayout.cshtml";
    var userName = Context.Session.GetString("UserName");
    var hub = Context.Session.GetString("Hub");
}
<script src="@Url.Content("https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js")" asp-append-version="true"></script>

<script src="@Url.Content("~/ckeditor/ckeditor.js")" asp-append-version="true"></script>
<script src="@Url.Content("~/js/jquery.validate.min.js")" asp-append-version="true"></script>
<script src="@Url.Content("~/js/jquery.validate.unobtrusive.min.js")" asp-append-version="true"></script>

<style>
    .btn-group > .btn {
        border-radius: 5px;
        border-color: #c6c6c6;
        height: 36px;
        text-align: left;
        font-size: 13px;
    }

    .multiselect-container.dropdown-menu {
        top: auto !important;
        bottom: 36px;
        max-height: 190px !important;
    }
</style>

<div class="card-box mb-0">
    @using (Html.BeginForm("NewProduction", "NewInitiation", FormMethod.Post, new { id = "Npd_Form_Submit", enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()

        @Html.HiddenFor(model => model.NpdHeaderTableData)
        @Html.HiddenFor(model => model.ProjectDetailsData)
        @Html.HiddenFor(model => model.ProductPositionigData)
        @Html.HiddenFor(model => model.FormulationProfileData)
        @Html.HiddenFor(model => model.PackagingProfileData)
        @Html.HiddenFor(model => model.BusinessInformationData)
        @Html.HiddenFor(model => model.SustainabilityData)
        @Html.HiddenFor(model => model.Status,new{@id="NpdStatus"})
        @Html.HiddenFor(model=>model.ApprovalStatus)
        @Html.HiddenFor(model=>model.IsEdit)
        @Html.HiddenFor(model => model.CurrentStatusName,new{@id="NpdCurrentStatusName"})
        @Html.HiddenFor(model => model.ProjectId,new{@id="ProjectId"})
        @Html.HiddenFor(model=>model.UserName,new{@Value=@userName,@id="UserName"})
        @Html.HiddenFor(model => model.SupportingDocumentData)
        @Html.HiddenFor(model => model.DeletedSupportingdocument)


        <div class="tb_clr marbt">
            <h3 class="cen_txt d-grid">
                NPD<span class="sub_head">(<strong>Note:</strong>  The Project Brief information will be auto saved for every 5 minutes . Please ensure that Project Name is entered to Auto save the data.)</span>
            </h3>
            <table id="NPD_Table" class="table table-bordered">
                <thead>
                    <tr class="mod_th">
                        <th scope="col">Hub</th>
                        <th scope="col">Division<span class="color-red">*</span><span id="Error_Npd_Division" class="color-red" style="display:none"><br />Please select Division</span></th>
                        <th scope="col">Category<span class="color-red">*</span><span id="Error_Npd_Category" class="color-red" style="display:none"><br />Please select Category</span></th>
                        <th scope="col">Initiated By</th>
                        <th scope="col">Initiated Date</th>
                        <th scope="col">Status</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td id="NPD_Hub">@hub</td>
                        <td id="NPD_Division">
                            @Html.DropDownListFor(model => model.Division,Model.DivisionList,"--Select--",new { @class = "form-select-sm", @id="Division"})
                            @Html.ValidationMessageFor(model => model.Division, "", new { @class = "text-danger" })
                        </td>
                        <td id="NPD_Category">
                            <select class="form-select-sm addCategoryOption data-singleselect" id="Npd_Category" name="Category">
                                <option value="">--Select--</option>
                                @Html.HiddenFor(model => model.Category, new { @class="form-control form-control-sm", @id="Category"} )
                            </select>
                            @Html.ValidationMessageFor(model => model.Category, "", new { @class = "text-danger" })
                            <span id="Message_Npd_Category" style="display:none"><br />Please select Division First</span>
                        </td>
                        <td id="NPD_InitiatedBy">@userName</td>
                        <td id="NPD_InitiatedDate"></td>
                        <td id="NPD_Status">New</td>
                    </tr>
                </tbody>
            </table>

            <h6 class="pro_det">Project Details</h6>
            <div class="row mb-3">
                <label class="col-sm-2 col-form-label">Project Name <span class="color-red">*</span></label>
                <div class="col-sm-10 pd">
                    @Html.TextBoxFor(model => model.ProjectName, new { @class = "form-control form-control-sm", @id="Npd_ProjectName"})
                    @*                    @Html.ValidationMessageFor(model => model.ProjectName, "", new { @class = "text-danger" })
                *@                    <span style="color:red; display:none" id="Error_Npd_ProjectName" class="Error_ProjectDetails">Please enter Project Name</span>
                </div>
            </div>
            <div class="row mb-3">
                <lable class="col-sm-2 col-form-label">Business Objective <span class="color-red">*</span></lable>
                <div class="ckediter_tolbar col-sm-10">
                    @Html.TextAreaFor(model => model.BusinessObjective, new { @name="editorpds", @class = "form-control", @id="Npd_BusinessObjective", @cols="30" })
                    @Html.ValidationMessageFor(model => model.BusinessObjective, "", new { @class = "text-danger" })
                    <span style="color:red; display:none" id="Error_Npd_BusinessObjective" class="Error_ProjectDetails">Please enter Business Objective </span>
                </div>
            </div>

            @*Product Positioning*@

            <h6 class="pro_det">Product Positioning</h6>
            <div class="row mb-3">
                <label class="col-sm-2 col-form-label">Target Consumer <span class="color-red">*</span></label>
                <div class="col-sm-10 pd">
                    @Html.TextBoxFor(model => model.ProductPositioningTargetConsumer, new { @class = "form-control form-control-sm", @id="PP_TargetConsumer"})
                    <span style="color:red; display:none" id="Error_PP_TargetConsumer" class="Error_ProductPositioning">Please enter Target Consumer </span>
                </div>
            </div>
            <div class="row mb-3">
                <lable class="col-sm-2 col-form-label">Competitive Offerings</lable>
                <div class="ckediter_tolbar col-sm-10">
                    @Html.TextAreaFor(model => model.ProductPositioningCompetitiveOfferings, new { @name="editor3", @class = "form-control", @id="PP_CompetitiveOfferings", @cols="30" })
                </div>
            </div>
            <div class="row mb-3">
                <lable class="col-sm-2 col-form-label">Unmet Need</lable>
                <div class="ckediter_tolbar col-sm-10">
                    @Html.TextAreaFor(model => model.ProductPositioningUnmetNeed, new { @name="editor4", @class = "form-control", @id="PP_UnmetNeed", @cols="30" })
                </div>
            </div>
            <span style="color:red;display:none" id="Error_ProductPositioning" class="Error_EmptyGrid">Please enter atleast one data for Product Positioning</span>
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-3">
                        <div class="form-group">
                            <lable class="col-form-label" style="position:relative; top:4px;">Product <span class="color-red">*</span></lable>
                            <div class="col-sm-12 mt-3">
                                @Html.TextBoxFor(model => model.ProductPositioningProduct, new { @class = "form-control ProductPositioning", @id="PP_Product", @style="border-color:#d1d1d1" })
                                <span style="color:red; display:none" id="Error_PP_Product" class="Error_ProductPositioning"></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-group">
                            <label class="col-form-label">Expected Features <span class="color-red">*</span></label>
                            <div class="ckediter_tolbar feature_benefits ">
                                @Html.TextAreaFor(model => model.ProductPositioningExpectedFeatures, new { @name="editor4", @class = "form-control ProductPositioning", @id="PP_ExpectedFeatures", @cols="30" })
                                <span style="color:red; display:none" id="Error_PP_ExpectedFeatures" class="Error_ProductPositioning">Please enter Expected Features </span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-group">
                            <label class="col-form-label">Expected Benefits <span class="color-red">*</span></label>
                            <div class="ckediter_tolbar feature_benefits">
                                @Html.TextAreaFor(model => model.ProductPositioningExpectedBenefits, new { @name="editor4", @class = "form-control ProductPositioning", @id="PP_ExpectedBenefits", @cols="30" })
                                <span style="color:red; display:none" id="Error_PP_ExpectedBenefits" class="Error_ProductPositioning">Please enter Expected Benefits </span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <lable class="col-form-label" style="position:relative; top:4px;">SKU <span class="color-red">*</span></lable>
                            <div class="col-sm-12 mt-3">
                                @Html.TextBoxFor(model => model.ProductPositioningSku, new { @class = "form-control ProductPositioning", @id="PP_Sku", @style="border-color:#d1d1d1" })
                                <span style="color:red; display:none" id="Error_PP_Sku" class="Error_ProductPositioning">Please enter SKU</span>
                                <span style="font-size:10px">(Please enter the SKU values with comma separated (Ex: 50 ml , 100 ml))</span>
                            </div>
                        </div>
                    </div>
                    @* <div class="col-md-1 pl-0">
                <button type="button" id="Add_ProductPositioning" class="btn-add margin_top115"> Add</button>
                </div>*@
