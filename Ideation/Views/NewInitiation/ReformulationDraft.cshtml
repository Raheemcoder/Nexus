                            <tr>
                                <td style="border: 1px solid black;width: 20%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>Existing Brand Name </b></td>
                                <td style="border: 1px solid black;width: 40%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>New Brand Name (if applicable)</b></td>
                                <td style="border: 1px solid black;width: 40%;padding: 5px 0px 5px 10px;font-weight: 600;text-align:center"><b>SKU </b></td>
                            </tr>

                        </thead>
                        <tbody>
                            @if (Model.ProducDescription != null)
                            {
                                foreach (var item in Model.ProducDescription)
                                {
                                    <tr>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.ProductName)</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.NewBrandName)</td>
                                        <td style="border: 1px solid black;padding: 5px 0px 5px 10px;">@Html.Raw(item.SKU)</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </td>
            </tr>



            <tr>
                <td>
