                        </div>
                        <div class="col-md-2 hide hideforpartusers" id="hgh-required-select-div">
                            <div class="form-group">
                                <label class="control-label">
                                    Is HGH Code Required <span class="text-danger">*</span>
                                </label>
                                @Html.DropDownListFor(model => model.IsHGHRequired, Model.IsHGHList, "Select",
                                new { @class = "form-control data-singleselect InfoIsHGHRequired", @id = "InfoIsHGHRequired" })
