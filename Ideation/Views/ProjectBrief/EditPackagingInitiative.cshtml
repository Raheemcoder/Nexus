@model Ideation.Models.PackageInitiatives;
@{
    ViewBag.Title = "New Package Initiative";
    Layout = "/Views/Shared/N_PB_Layout.cshtml";

    var userName = Context.Session.GetString("UserName");
    var hub = Context.Session.GetString("Hub");
}
<section>
    <div class="">
        <div class="-m_title_ ">
            <h4 id="Page_Heading"></h4>
        </div>

        <div class="note_savedata_">
            @if ((Model.ID == "1" || Model.ID == "8" || Model.ID == "11") && Model.Icon != "View")
            {
                <span>
                    (Note: The Project Brief information will be auto saved for every 5 minutes . Please ensure that Project Name is entered to Auto save the data.)
                </span>
            }
        </div>

        <div class="right_top mt-0">
            <ul>
                @if (Model.Icon != "View")
                {
                    <li class="preview" data-toggle="modal" data-target="#preview"><i class="fa fa-eye"></i> Preview</li>
                }
                @if (Model.Page != "HGH")
                {
                    <li><a href="@Url.Content("~/ProjectBrief/ProjectBrief")"><i class="fas fa-arrow-left"></i> Back</a></li>
                }
            </ul>
        </div>
    </div>
</section>

@using (Html.BeginForm("EditPackagingInitiative", "ProjectBrief", FormMethod.Post, new { @class = "width_100", id = "PackageEdit_Form_Submit", enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(model => model.JsonData)
    @Html.HiddenFor(model => model.JsonFormPackHUbBusinessData)
    @Html.HiddenFor(model => model.JsonPackHUBRemarksData)
    @Html.HiddenFor(model => model.JsonPMDReview)
    @Html.HiddenFor(model => model.JsonPackHgmlToHubData)
    @Html.HiddenFor(model => model.JsonPackHgmlToOtherHubData)
    @Html.HiddenFor(model => model.UserName, new { @Value = @userName, @id = "UserName" })
    @Html.HiddenFor(model => model.ProjectId, new { @id = "ProjectId" })
    @Html.HiddenFor(model => model.PackageHeaderTableData)
    @Html.HiddenFor(model => model.PackageProjectDetails)
    @Html.HiddenFor(model => model.PackageProductDescription)
    @Html.HiddenFor(model => model.PackageBusinessInformation)
    @Html.HiddenFor(model => model.PackageExpextedPackagingProfile)
    @Html.HiddenFor(model => model.PackageSustainability)
    @Html.HiddenFor(model => model.StatusId, new { @id = "PackStatus" })
    @Html.HiddenFor(model => model.Hubs)
    @Html.HiddenFor(model => Model.HubList)
    @Html.HiddenFor(model => model.HgmlData)
    @Html.HiddenFor(model => model.ApprovalStatus)
    @Html.HiddenFor(model => model.packageSendBackToInitiatorRemarks)
    @Html.HiddenFor(model => model.PackSendToPmdRemarks)
    @Html.HiddenFor(model => model.PackSendToHubRemarks)
    @Html.HiddenFor(model => model.PackRejectRemarks)
    @Html.HiddenFor(model => model.PackageInitiatorRemarks)
    @Html.HiddenFor(model => model.Hub, new { @id = "PackHub" })
    @Html.HiddenFor(model => model.SendToHGMLApproveRemarks)
    @Html.HiddenFor(model => model.CurrentStatus)
    @Html.HiddenFor(model => model.IsHubApproved)
    @Html.HiddenFor(model => model.PMDData)
    @Html.HiddenFor(model => model.PackPMDDataApproveRemarks)
    @Html.HiddenFor(model => model.PMdDueDate)
    @Html.HiddenFor(model => model.Status, new { @id = "ProjectStatus" })
    @Html.HiddenFor(model => model.Hub)
    @Html.HiddenFor(model => model.HubListJson)
    @Html.HiddenFor(model => model.HubUsers)
    @Html.HiddenFor(model => model.ViewStatus)
    @Html.HiddenFor(model => model.ID, new { @id = "ProjectstatusID" })
    @Html.HiddenFor(model => model.ReceivedDate, new { @id = "ReceivedDate" })
    @Html.HiddenFor(model => model.TargetCostGridData, new { @id = "TargetCostGridData" })
    @Html.HiddenFor(model => model.SavedRemarks, new { @id = "SavedRemarks" })
    @Html.HiddenFor(model => model.DeletedRemarks, new { @id = "DeletedRemarks" })

    @Html.HiddenFor(model => model.SupportingDocumentData)
    @Html.HiddenFor(model => model.DeletedSupportingdocument)
    @Html.HiddenFor(model => model.Page)

    <div>
        <section>
            <div class="">

                <div class="-m_title_ ">
                    @if (Model.Status == "View")
                    {
                        <h4 class="cen_txt" id="statusHeader"></h4>
                    }
                    @if (Model.ID == "1")
                    {
                        <h4>Packaging Initiative-Draft</h4>

                    }
                    @if (Model.ID == "2")
                    {
                        <h4>Packaging Initiative-HGML Review</h4>
                    }
                    @if (Model.ID == "3")
                    {
                        <h4>Packaging Initiative-HUB Review</h4>
                    }
                    @if (Model.ID == "4")
                    {
                        <h4>Packaging Initiative-HGML Approve</h4>

                    }

                    @if (Model.ID == "5")
                    {
                        <h4>Packaging Initiative-Fine Screening Review</h4>

                    }
                    @if (Model.ID == "16")
                    {
                        <h4>Packaging Initiative-Extended Fine Screening Review</h4>

                    }
                    @if (Model.ID == "6")
                    {
                        <h4>Packaging Initiative-Accepted</h4>

                    }
                    @if (Model.ID == "7")
                    {
                        <h4>Packaging Initiative-Rejected</h4>

                    }
                    @if (Model.ID == "8")
                    {
                        <h4>Packaging Initiative-Sent Back To Initiator</h4>

                    }
                    @if (Model.ID == "9")
                    {
                        <h4>Packaging Initiative-My approval Pending</h4>

                    }
                    @if (Model.ID == "11")
                    {
                        <h4>Packaging Initiative-Brief Demoted to Initiator</h4>

                    }
                    @if (Model.ID == "12")
                    {
                        <h4>Packaging Initiative-Updated</h4>

                    }
                    @if (Model.ID == "13" || Model.ID == "14")
                    {
                        <h4>Packaging Initiative-Brief Demoted to HGML</h4>

                    }
                </div>

            </div>
        </section>

        <section>
            <div class="d-flex">

                @if (Model.ID == "1" || Model.ID == "8" || Model.ID == "9" || Model.ID == "11")
                {
                    <div class="initiated_section">
                        <div class="hub_name PackageInitiative_hub">@hub</div>
                        <div class="form_initiated">
                            <div class="col-initiated">
                                <div class="">
                                    <label class="control-label">Division <span class="color-red">*</span></label>
                                    <div class="_init_width_dropdown">
                                        @Html.DropDownListFor(model => model.Division, Model.DivisionList, "--Select--", new { @class = "form-select-sm", @id = "Division", @data_singleselect = "data-singleselect" })
                                        <span id="Error_Pack_Division" class="color-red" style="display:none"><br />Please select Division</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated">
                                <div class="">
                                    <label class="control-label">Category <span class="color-red">*</span></label>
                                    <div class="_init_width_dropdown">
                                        <select class="form-control trim addCategoryOption" data-singleselect>
                                            <option value="">---Select---</option>
                                        </select>
                                        @Html.HiddenFor(model => model.Category, new { @class = "form-control trim form-control trim-sm", @id = "Category" })
                                        <span id="Error_pack_Category" class="color-red" style="display:none"><br />Please select Category</span>
                                        <span id="Message_Pack_Category" style="display:none"><br />Please select Division First</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <label class="control-label">Initiated By : </label>
                                    <div class="">
                                        <span id="PackageInitiative_InitiatedBy">@userName</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <label class="control-label"> Initiated Date :</label>
                                    <div class="">
                                        <span id="PackageInitiative_InitatedOn"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <label class="control-label">Status : </label>
                                    <div class="status_value">
                                        <span id="PackageInitiative_Status"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <span>Remarks <a href="@Url.Content("#")" data-bs-toggle="modal" onclick="onClickOfApprovalRemarks()" title="Remarks"> <i class="fa fa-info-circle" aria-hidden="true"></i></a></span>
                                </div>
                            </div>
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <a>Download <i class="fa fa-download downloadPackageinitiativePdf" title="Download"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div>
                        <div class="expand_initiate">
                            <div>
                                <div onclick="$(document).toggleFullScreen()" class="js-toggle-fullscreen-btn toggle-fullscreen-btn" aria-label="Enter fullscreen mode">
                                    <i class="fas fa-expand fullscreen_expand fullscreen-btn" title="Full Screen"></i>
                                    <i class="fas fa-times fullscreen_minimize exitscreen-btn"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                }
                @if (Model.ID == "2" || Model.ID == "3" || Model.ID == "4" || Model.ID == "5" || Model.ID == "16" || Model.ID == "6" || Model.ID == "7" || Model.ID == "12" || Model.ID == "13" || Model.ID == "14" || Model.Status == "View")
                {
                    <div class="initiated_section">
                        <div class="hub_name PackageInitiative_hub">@hub</div>
                        <div class="form_initiated">
                            <div class="col-initiated">
                                <div class="">
                                    <label class="control-label">Division <span class="color-red">*</span></label>
                                    <div class="_init_width_dropdown">
                                        @Html.DropDownListFor(model => model.Division, Model.DivisionList, "--Select--", new { @class = "form-select-sm", @data_singleselect = "data-singleselect", @id = "Division", @disabled = "disabled" })
                                        @Html.ValidationMessageFor(model => model.Division, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated">
                                <div class="">
                                    <label class="control-label">Category <span class="color-red">*</span></label>
                                    <div class="_init_width_dropdown">
                                        <select class="form-control trim addCategoryOption" data-singleselect disabled>
                                            <option value="">Select</option>
                                        </select>
                                        @Html.HiddenFor(model => model.Category, new { @class = "form-control trim form-control trim-sm", @id = "Category" })
                                        @Html.ValidationMessageFor(model => model.Category, "", new { @class = "text-danger" })
                                        <span id="Message_Pack_Category" style="display:none"><br />Please select Division First</span>
                                    </div>
                                </div>
                            </div>
                            @if (Model.ID == "3" || Model.Status == "View")
                            {
                                <div class="col-initiated forhubonly">
                                    <div class="">
                                        <label class="control-label forhubonly">HUB Approve <span class="color-red">*</span></label>
                                        <div class="_init_width_dropdown forhubonly" id="PackageInitiative_IsHubApprove">
                                            @Html.DropDownListFor(model => model.IsHubApproved, new List<SelectListItem>(){
                                            new SelectListItem() { Text= "--Select--", Value = "",Selected =true  },
                                                new SelectListItem() { Text= "Yes", Value = "Yes" },
                                                new SelectListItem() { Text= "No", Value = "No" }
                                                }, new { @class = "form-control data-singleselect", @id = "PackHubApprove" })
                                            <span id="spanhubApprove" class="validatespan" style="color:red;display:none">Please Select Hub Approve</span>
                                        </div>
                                    </div>
                                </div>
                            }
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <label class="control-label">Initiated By: </label>
                                    <div class="" id="PackageInitiative_InitiatedBy">
                                        <span>@userName</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <label class="control-label"> Initiated Date:</label>
                                    <div class="" id="PackageInitiative_InitatedOn">
                                        <span></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <label class="control-label">Status: </label>
                                    <div class="status_value" id="PackageInitiative_Status">
                                        <span></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <label class="control-label">Remarks: </label>
                                    <div class="">
                                        <a href="@Url.Content("#")" data-bs-toggle="modal" onclick="onClickOfApprovalRemarks()" title="Remarks"><i class="fa fa-info-circle" aria-hidden="true"></i></a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-initiated by_date_iniitia">
                                <div class="d-flex align-items-center">
                                    <label class="control-label">Download: </label>
                                    <div class="">
                                        <div><a><i class="fa fa-download downloadPackageinitiativePdf" title="Download"></i></a></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div>
                        <div class="expand_initiate">
                            <div>
                                <div onclick="$(document).toggleFullScreen()" class="js-toggle-fullscreen-btn toggle-fullscreen-btn" aria-label="Enter fullscreen mode">
                                    <i class="fas fa-expand fullscreen_expand fullscreen-btn "></i>
                                    <i class="fas fa-times fullscreen_minimize exitscreen-btn"></i>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </section>

        <section>
            <div class="button_top float-right mt-2">
                @if (Model.IsEdit)
                {
                    @if (Model.ID == "1")
                    {
                        <button type="button" class="btn btn-draft PackagingInit_Save" data-bs-toggle="modal" data-title="Save" onclick="ValidatePackSaveForm()">Save</button>
                    }
                    @if (Model.ID == "2" || Model.ID == "3" || Model.ID == "4" || Model.ID == "5" || Model.ID == "16" || Model.ID == "8" || Model.ID == "11" || Model.ID == "13" || Model.ID == "14")
                    {
                        <button type="button" class="btn btn-draft PackagingInit_Save" data-bs-toggle="modal" data-title="Save" onclick="ValidatePackSaveForm()"> Save</button>
                    }

                    @if (Model.ID == "2" || Model.ID == "13")
                    {
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="PackageInitiativeSendBackForm()" data-title="Send Back">Send back</button>
                    }
                    @if (Model.ID == "5" || Model.ID == "16")
                    {
                        <button type="button" class="btn btn btn-hgml" data-bs-toggle="modal" onclick="PMDSendBacktoInitiator()" data-title="Send Back">Send back to Initiator</button>
                        <button type="button" class="btn btn btn-hgml" data-bs-toggle="modal" onclick="PackageInitiativeSendBackForm()" data-title="Send Back">Send back to HGML</button>
                    }
                    @if (Model.ID == "2" || Model.ID == "4" || Model.ID == "13" || Model.ID == "14")
                    {

                        <button type="button" class="btn btn-hgml SendToHUB" data-bs-toggle="modal" onclick="validateSendToHubForm()" data-title="Send To HUB">Send To HUB</button>
                        <button type="button" class="btn btn-hgml SendtoPMD" data-bs-toggle="modal" onclick="validateSendToPMDForm()" data-title="Approve"> Send To PMD</button>
                        <button type="button" class="btn btn-hgml SendtoUnderExploration" data-bs-toggle="modal" onclick="ValidatePackSubmitToExplorationFromHGML()" data-title="Approve" style="display:none">Send To Extended Fine Screening Review</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="PackageInitiativeReject()" data-title="Reject">Reject</button>

                    }
                    @if (Model.ID == "3")
                    {


                        <button type="button" class="btn btn-hgml test" onclick="ValidatesentTOHGMLApproveForm()">Send To HGML</button>


                    }
                    @if (Model.ID == "1" || Model.ID == "8")
                    {
                        <button type="button" class="btn btn-approval PackagingInit_submittoManager" data-title="Submit" onclick="ValidatePackSubmitToManager()">Send for Approval</button>
                        <button type="button" class="btn btn-hgml HideIfDemoted PackagingInit_Submit" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitForm()">Send to HGML </button>
                        <button type="button" class="btn btn-hgml SendtoPMDInSendBack" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitToPMD()" style="display:none">Send to PMD </button>
                        <button type="button" class="btn btn-hgml SendtoUnderExplorationInSendBack SendtoUnderExploration" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitToExploration()" style="display:none">Send To Extended Fine Screening Review</button>

                    }
                    @if (Model.ID == "11")
                    {

                        <button type="button" class="btn btn-approval PackagingInit_submittoManager" data-title="Submit" onclick="ValidatePackSubmitToManager()">Send for Approval</button>
                        <button type="button" class="btn btn-hgml sendToPMDinDemoted SendtoPMDInSendBack" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitToPMD()" style="display:none">Send to PMD </button>
                        <button type="button" class="btn btn-hgml SendtoUnderExplorationInSendBack SendtoUnderExploration" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitToExploration()" style="display:none">Send to Extended Fine Screening Review</button>


                    }
                    @if (Model.ID == "5")
                    {
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validateSendToUnderExploration()" data-title="Send Mail"> Send To Extended Fine Screening Review</button>

                    }
                    @if (Model.ID == "5" || Model.ID == "16")
                    {


                        <button type="button" class="btn btn-approval" data-bs-toggle="modal" onclick="ValidateAcceptForm()" data-title="Approve"> Accept</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="PackageInitiativeReject()" data-title="Reject">Reject</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validatePmdReviewSendMailForm()" data-title="Send Mail"> Send Mail</button>

                    }

                    @if (Model.ID == "9")
                    {

                        <button type="button" class="btn btn-draft PackagingInit_Save" data-bs-toggle="modal" data-title="Save" onclick="ValidatePackSaveForm()"> Save</button>
                        <button type="button" class="btn btn btn-hgml" data-bs-toggle="modal" onclick="PackageInitiativeSendBackForm()" data-title="Send Back">Send back</button>
                        <button type="button" class="btn btn-hgml HideIfDemoted PackagingInit_Submit" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitForm()">Send To HGML</button>
                        <button type="button" class="btn btn-hgml SendtoPMDInApproval" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitToPMD()" style="display:none">Send to PMD </button>
                        <button type="button" class="btn btn-hgml SendtoUnderExploration" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitToExploration()" style="display:none">Send To Fine Screening</button>


                    }
                    @if (Model.ID == "6" || Model.ID == "12")
                    {
                        <button type="button" class="btn btn-draft PackagingInit_SaveAndUpdate" data-bs-toggle="modal" data-title="Save" onclick="ValidatePackSaveandUpdate()"> Save and Update</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validatePmdReviewSendMailForm()" data-title="Send Mail"> Send Mail</button>

                    }

                    @if (Model.ID == "1" || Model.ID == "2" || Model.ID == "3" || Model.ID == "4" || Model.ID == "5" || Model.ID == "16" || Model.ID == "6" || Model.ID == "7" || Model.ID == "8" || Model.ID == "9" || Model.ID == "12" || Model.ID == "11" || Model.ID == "13" || Model.ID == "14" || Model.Status == "View")
                    {

                        <button type="button" class="btn btn-cancel PackagingInit_Cancel" data-bs-toggle="modal" data-bs-target="#CancelModal" data-title="Cancel">Back</button>

                    }
                }
            </div>
            <div>
                <ul class="link_href mb-2 mt-2">
                    <li><a href="#ci" onclick="moveToTop('ci', 'highlight_section')" class="active">Project Name</a></li>
                    <li><a href="#bh" onclick="moveToTop('bh', 'highlight_section')">Product Description</a></li>
                    <li><a href="#sd" onclick="moveToTop('sd', 'highlight_section')">Project Details</a></li>
                    <li><a href="#ship_location" onclick="moveToTop('ship_location', 'highlight_section')">Business Information</a></li>
                    <li><a href="#tc" onclick="moveToTop('tc', 'highlight_section')">Product Profile:Packaging Profile</a></li>
                    <li><a href="#ship_address" onclick="moveToTop('ship_address', 'highlight_section')">Sustainability</a></li>
                    <li><a href="#sd1" onclick="moveToTop('sd1', 'highlight_section')">Supporting Documents</a></li>
                    @if (Model.ID == "1" || Model.ID == "8" || Model.ID == "11")
                    {
                        <li><a href="#IR" onclick="moveToTop('IR', 'highlight_section')">Initiator Remarks</a></li>
                    }
                    @if (Model.ID == "2")
                    {
                        <li><a href="#sh1" onclick="moveToTop('sh1', 'highlight_section')">HGML Data</a></li>

                    }
                    @if (Model.ID == "3")
                    {
                        <li><a href="#sh1" onclick="moveToTop('sh1', 'highlight_section')">HGML/HUB Data</a></li>

                    }
                    @if (Model.ID == "4")
                    {
                        <li><a href="#hbi" onclick="moveToTop('hbi', 'highlight_section')">HUB Status</a></li>
                        <li><a href="#hgdata1" onclick="moveToTop('hgdata1', 'highlight_section')">HGML Data</a></li>
                    }
                    @if (Model.ID == "5")
                    {
                        <li><a href="#sd11" onclick="moveToTop('sd11', 'highlight_section')">HGML Data</a></li>
                        <li><a href="#sd1" onclick="moveToTop('sd1', 'highlight_section')">Fine Screening Data</a></li>
                        <li><a href="#tc1" onclick="moveToTop('tc1', 'highlight_section')">Target Cost</a></li>

                    }
                </ul>
            </div>

            <div class="cust_fields_master">
                <div id="ci" class="highlight_section ship_to">
                    <div class="details_section">
                        <h4>Project Name</h4>
                        <div class="form_section">

                            @if (Model.ID == "1" || Model.ID == "8" || Model.ID == "9" || Model.ID == "11")
                            {
                                <div class="form-group">
                                    <label class="control-label">Project Name <span class="color-red">*</span></label>
                                    @Html.TextBoxFor(m => m.ProjectName, new { id = "ProductDescription_ProjectName", @class = "form-control trim trim form-select-sm " })
                                    @Html.ValidationMessageFor(model => model.ProjectName, "", new { @class = "text-danger" })
                                    <span style="color:red" id="Error_in_ProjectName" hidden>Please enter Project Name</span>

                                </div>
                            }
                            else
                            {
                                <div class="form-group">
                                    <label class="control-label">Project Name <span class="color-red">*</span></label>
                                    @Html.TextBoxFor(m => m.ProjectName, new { id = "ProductDescription_ProjectName", @class = "form-control trim trim form-select-sm ", @readonly = "readonly" })
                                </div>
                            }
                        </div>

                    </div>
                </div>
                <div id="bh" class="ship_to highlight_section">
                    <div class="details_section ">
                        <h4>Product Description</h4>
                        <div class="form_section">
                            @if (Model.ID == "1" || Model.ID == "8" || Model.ID == "9" || Model.ID == "11" || Model.Status == "View")
                            {
                                <div id="productdescriptionMsg" style="color:red" hidden>Please enter atleast one data for Product Description</div>
                                <div class="product_packag product_profile">
                                    <div class="row onlyfor8">
                                        <div class="col-md-3 ">
                                            <div class="form-group">
