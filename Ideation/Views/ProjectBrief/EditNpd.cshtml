@model Ideation.Models.NPD;

@{
    ViewData["Title"] = "EditNpd";
    Layout = "/Views/Shared/N_PB_Layout.cshtml";
    var userName = Context.Session.GetString("UserName");
    var hub = Context.Session.GetString("Hub");
    Model.UserName = userName;

}
<style>
    #Division-error, #Npd_Category-error {
        color: red;
    }
</style>

<section>
    <div class="">
        <div class="-m_title_ ">
            <h4 id="Page_Heading"></h4>
        </div>

        <div class="note_savedata_">
            @if ((Model.CurrentStatusId == "1" || Model.CurrentStatusId == "8" || Model.CurrentStatusId == "11") && Model.Icon != "View")
            {
                <span>
                    (Note: The Project Brief information will be auto saved for every 5 minutes.Please ensure that Project Name is entered to Auto save the data.)
                </span>
            }
        </div>
        <div class="right_top">
            <ul>
                @if (Model.Icon != "View")
                {
                    <li class="preview" data-toggle="modal" data-target="#preview"><i class="fa fa-eye"></i> Preview</li>

                    <li><a href="@Url.Content("~/ProjectBrief/ProjectBrief")"><i class="fas fa-arrow-left"></i> Back</a></li>
                }
            </ul>
        </div>
    </div>
</section>

@using (Html.BeginForm("EditNpd", "ProjectBrief", FormMethod.Post, new { @class = "width_100", @id = "Npd_Edit_Form_Submit", enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(model => model.JsonFormNpdData)
    @Html.HiddenFor(model => model.JsonFormNpdHgmlReviewData)
    @Html.HiddenFor(model => model.JsonFormNpdHubReviewData)
    @Html.HiddenFor(model => model.JsonFormNpdHgmlApproveData)
    @Html.HiddenFor(model => model.JsonFormNpdPmdReviewData)

    @Html.HiddenFor(model => model.UserName, new { @Value = @userName, @id = "UserName" })

    @Html.HiddenFor(model => model.ProjectId, new { @id = "ProjectId" })
    @Html.HiddenFor(model => model.StatusNameDb, new { @id = "StatusNameDb" })
    @Html.HiddenFor(model => model.NpdHeaderTableData)
    @Html.HiddenFor(model => model.ProjectDetailsData)
    @Html.HiddenFor(model => model.ProductPositionigData)
    @Html.HiddenFor(model => model.FormulationProfileData)
    @Html.HiddenFor(model => model.PackagingProfileData)
    @Html.HiddenFor(model => model.BusinessInformationData)
    @Html.HiddenFor(model => model.SustainabilityData)
    @Html.HiddenFor(model => model.PmdData)
    @Html.HiddenFor(model => model.TargetCostData, new { @id = "NpdTargetCostData" })
    @Html.HiddenFor(model => model.HgmlApproveSendBackHubRemarksData)

    @Html.HiddenFor(model => model.HgmlData)
    @Html.HiddenFor(model => model.Status, new { @id = "NpdStatus" })
    @Html.HiddenFor(model => model.ApprovalStatus)
    @Html.HiddenFor(model => model.SendToHubRemarks)
    @Html.HiddenFor(model => model.SendToPmdRemarks)
    @Html.HiddenFor(model => model.SendBackToInitiatorRemarks)
    @Html.HiddenFor(model => model.RejectRemarks)
    @Html.HiddenFor(model => model.SendToHgmlRemarks)

    @Html.HiddenFor(model => model.ConfirmationRemarks)

    @Html.HiddenFor(model => model.CurrentStatusName, new { @id = "NpdCurrentStatusName" })

    @Html.HiddenFor(model => model.Icon, new { @id = "IconName" })
    @Html.HiddenFor(model => model.IsEdit)

    @Html.HiddenFor(model => model.ReceivedDate, new { @id = "ReceivedDate" })
    @Html.HiddenFor(model => model.SavedRemarks, new { @id = "SavedRemarks" })
    @Html.HiddenFor(model => model.DeletedRemarks, new { @id = "DeletedRemarks" })

    @Html.HiddenFor(model => model.SupportingDocumentData)
    @Html.HiddenFor(model => model.DeletedSupportingdocument)

    @Html.HiddenFor(model => model.PMdDueDate)
    @Html.HiddenFor(model => model.Page)
    <section>
        <div class="d-flex">
            <div class="initiated_section Npd_HeaderTable" id="NPD_Table">
                <div class="hub_name" id="NPD_Hub">@hub </div>
                <div class="form_initiated">
                    <div class="col-initiated">
                        <div class="" id="NPD_Division">
                            <label class="control-label">Division <span class="color-red">*</span></label>
                            <div class="_init_width_dropdown">
                                @Html.DropDownListFor(model => model.Division, Model.DivisionList, "--Select--", new { @class = "form-control data-singleselect", @id = "Division" })
                                <span id="ErrorDivision" class="color-red" style="display:none"><br />Please select Division</span>
                                @Html.ValidationMessageFor(model => model.Division, "", new { @class = "color-red" })
                            </div>
                        </div>
                    </div>
                    <div class="col-initiated">
                        <div class="" id="NPD_Category">
                            <label class="control-label">Category <span class="color-red">*</span></label>
                            <div class="_init_width_dropdown">
                                <select class="form-control addCategoryOption data-singleselect" id="Npd_Category" name="Category">
                                    <option value="">--Select--</option>
                                    @Html.HiddenFor(model => model.Category, new { @class = "form-control form-control-sm", @id = "Category" })
                                </select>
                                @Html.ValidationMessageFor(model => model.Category, "", new { @class = "color-red" })
                                <span id="Message_Npd_Category" style="display:none"><br />Please select Division First</span>
                                <span id="ErrorCategory" class="color-red" style="display:none"><br />Please select Category</span>
                            </div>
                        </div>
                    </div>
                    @if (Model.CurrentStatusId == "3")
                    {
                        <div class="col-initiated NotInNpd NotInNpdHgmlReview NotInHgmlApprove NotInPmdReview NotInApproved">
                            <div class="" id="Npd_HubApproveConfirmation">
                                <label class="control-label">
                                    HUB Approve
                                </label><span class="color-red">*</span>
                                <div class="_init_width_dropdown">
                                    @Html.DropDownListFor(model => model.HubApproveConfirmation, new List<SelectListItem>()
                                    {
                                    new SelectListItem() { Text= "--Select--", Value = "",Selected =true  },
                                    new SelectListItem() { Text= "Yes", Value = "Yes" },
                                    new SelectListItem() { Text= "No", Value = "No" }
                                    }, new { @id = "HubApproveConfirmation", @class = "form-control data-singleselect" })
                                    <span id="Error_HubApprove" class="color-red" style="display:none">Please select HUB Approve</span>
                                </div>

                            </div>
                        </div>
                    }
                    <div class="col-initiated by_date_iniitia">
                        <div class="d-flex align-items-center">
                            <label class="control-label">Initiated By: </label>
                            <div class="" id="NPD_InitiatedBy">
                                <span>@userName</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-initiated by_date_iniitia">
                        <div class="d-flex align-items-center">
                            <label class="control-label"> Initiated Date:</label>
                            <div class="" id="NPD_InitiatedDate">
                                <span></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-initiated by_date_iniitia">
                        <div class="d-flex align-items-center">
                            <label class="control-label">Status: </label>
                            <div class="status_value" id="NPD_Status">
                                New
                            </div>
                        </div>
                    </div>

                    <div class="col-initiated by_date_iniitia">
                        <div class="d-flex align-items-center">
                            <label class="control-label">Remarks: </label>
                            <div class=""><a href="@Url.Content("#")" data-bs-toggle="modal" onclick="onClickOfApprovalRemarks()"><i class="fa fa-info-circle" aria-hidden="true" title="Remarks"></i></a></div>
                        </div>
                    </div>
                    <div class="col-initiated by_date_iniitia">
                        <div class="d-flex align-items-center">
                            <label class="control-label">Download: </label>
                            <div><a><i class="fa fa-download downloadPdf" aria-hidden="true" title="Download"></i></a></div>
                        </div>
                    </div>
                </div>
            </div>
            <div>
                <div class="expand_initiate">
                    <div>
                        <div onclick="$(document).toggleFullScreen()" class="js-toggle-fullscreen-btn toggle-fullscreen-btn" aria-label="Enter fullscreen mode">
                            <i class="fas fa-expand fullscreen_expand fullscreen-btn" title="Full Screen"></i>
                            <i class="fas fa-times fullscreen_minimize exitscreen-btn"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section>
    </section>
    <section>
        @if (Model.Icon != "View")
        {
            @if (Model.CurrentStatusId == "1" || Model.CurrentStatusId == "8" || Model.CurrentStatusId == "9")
            {
                <div class="button_top float-right mt-2 NotInNpdHgmlReview NotInHubReview NotInHgmlApprove NotInPmdReview">

                    @if (Model.CurrentStatusId != "9")
                    {
                        @if (Model.IsEdit)
                        {
                            <button type="button" id="NPD_Save" class="btn btn-draft NotInApproved" data-bs-toggle="modal" data-title="Save" onclick="validateNpdSaveForm()">Save</button>
                            <button type="button" id="NPD_Submit" class="btn btn-hgml NotInApproved" data-bs-toggle="modal" data-title="Submit" onclick="ValidateSendToManagerForm()">Send For Approval</button>
                            <button type="button" id="NPD_Submit" class="btn btn-hgml NotInApproved HideIfDemoted" data-bs-toggle="modal" data-title="Submit" onclick="validateNpdSubmitForm()">Send To HGML</button>
                            <button type="button" class="btn btn-hgml NotInRejected SendToPmdSendback_" id="SendToPmdSendback_" data-bs-toggle="modal" onclick="validateInitiatorsendtoPmd()" data-title="Send To PMD" style="display:none"> Send To PMD</button>
                            <button type="button" id="SendtoUnderExploration" class="btn btn-hgml SendtoUnderExploration SendtoUnderExplorationInSendBack" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitToExploration()" style="display:none">Send to PMD </button>
                        }
                    }
                    @if (Model.CurrentStatusId == "9")
                    {
                        @if (Model.IsEdit)
                        {
                            <button type="button" id="NPD_Save" class="btn btn-draft NotInApproved" data-bs-toggle="modal" data-title="Save" onclick="validateNpdPendingApprovalSaveForm()">Save</button>
                            <button type="button" class="btn btn-hgml NotInRejected" data-bs-toggle="modal" onclick="validateNpdsendbackForm()" data-title="Send Back">Send Back</button>
                            <button type="button" id="NPD_Submit" class="btn btn-hgml NotInApproved HideIfDemoted" data-bs-toggle="modal" data-title="Submit" onclick="validateNpdSubmitForm()">Send To HGML</button>
                            <button type="button" class="btn btn-hgml NotInRejected SendToPmd_" id="SendToPmd_" data-bs-toggle="modal" onclick="validateInitiatorsendtoPmd()" data-title="Send To PMD" style="display:none"> Send To PMD</button>
                            <button type="button" id="SendtoUnderExploration" class="btn btn-hgml SendtoUnderExploration SendtoUnderExplorInApproval" data-bs-toggle="modal" data-title="SendtoUnderExploration" onclick="ValidatePackSubmitToExploration()" style="display:none">Send to PMD </button>

                        }
                    }
                    <button type="button" id="NPD_Cancel" class="btn btn-cancel" data-bs-toggle="modal" data-bs-target="#CancelModal" data-title="Back"> Back</button>
                </div>

            }

            @if (Model.CurrentStatusId == "2" || Model.CurrentStatusId == "13")
            {
                <div class="button_top float-right mt-2 NotInNpd NotInHubReview NotInHgmlApprove NotInPmdReview NotInApproved">

                    @if (Model.IsEdit)
                    {
                        <button type="button" class="btn btn-draft NotInRejected" data-bs-toggle="modal" onclick="validateHgmlReviewSaveForm()" data-title="Save">Save</button>
                        <button type="button" class="btn btn-hgml NotInRejected" data-bs-toggle="modal" onclick="validateHgmlReviewSendBackInitiatorForm()" data-title="Send Back">Send Back</button>
                        <button type="button" class="btn btn-hgml NotInRejected Button_SendToHub" id="Button_SendToHub" data-bs-toggle="modal" onclick="validateHgmlReviewSendToHubForm()">Send To HUB</button>
                        <button type="button" class="btn btn-hgml NotInRejected Button_SendToPmd" id="Button_SendToPmd" data-bs-toggle="modal" onclick="validateHgmlReviewSendToPmdForm()" data-title="Send To PMD"> Send To PMD</button>
                        <button type="button" class="btn btn-hgml SendtoUnderExploration" id="SendtoUnderExploration" data-bs-toggle="modal" onclick="ValidateNpdSubmitToExplorationFromHGML()" data-title="Approve" style="display:none"> Send To PMD</button>

                        <button type="button" class="btn btn-hgml NotInRejected" data-bs-toggle="modal" onclick="validateHgmlReviewRejectForm()" data-title="Reject">Reject</button>

                    }
                    <button type="button" class="btn btn-cancel" data-bs-toggle="modal" data-bs-target="#CancelModal" data-title="Back">Back</button>
                </div>

            }

            @if (Model.CurrentStatusId == "3")
            {
                <div class="button_top float-right mt-2 NotInNpd NotInNpdHgmlReview NotInHgmlApprove NotInPmdReview NotInApproved">

                    @if (Model.IsEdit)
                    {
                        <button type="button" class="btn btn-draft" data-bs-toggle="modal" onclick="validateHubReviewSaveForm()" data-title="Save">Save</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validateHubReviewSendToHgmlForm()" data-title="Send To HGML">Send To HGML</button>

                    }
                    <button type="button" class="btn btn-cancel" data-bs-toggle="modal" data-bs-target="#CancelModal" data-title="Back">Back</button>
                </div>

            }

            @if (Model.CurrentStatusId == "4" || Model.CurrentStatusId == "14")
            {
                <div class="button_top float-right mt-2 NotInNpd NotInNpdHgmlReview NotInHubReview NotInPmdReview NotInApproved">

                    @if (Model.IsEdit)
                    {
                        <button type="button" class="btn btn-draft" data-bs-toggle="modal" onclick="validateHgmlApproveSaveForm()" data-title="Save"> Save</button>
                        <button type="button" class="btn btn-hgml SendtoPMD" id="SendtoPMD" data-bs-toggle="modal" onclick="validateHgmlApproveSendToPmdForm()" data-title="Send To PMD"> Send To PMD</button>
                        <button type="button" class="btn btn-hgml SendtoUnderExploration" id="SendtoUnderExploration" data-bs-toggle="modal" onclick="ValidateNpdSubmitToExplorationFromHGML()" data-title="Approve" style="display:none"> Send To PMD</button>

                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validateHgmlApproveSendBackToHubForm()" data-title="Send To HUB">Send To HUB</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validateHgmlApproveRejectForm()" data-title="Reject">Reject</button>

                    }
                    <button type="button" class="btn btn-cancel" data-bs-toggle="modal" data-bs-target="#CancelModal" data-title="Back">Back</button>
                </div>

            }

            @if ((Model.CurrentStatusId == "6" || Model.CurrentStatusId == "12") && Model.Icon != "View")
            {
                <div class="button_top float-right mt-2 NotInNpd NotInNpdHgmlReview NotInHubReview NotInHgmlApprove">

                    @if (Model.IsEdit)
                    {
                        <button type="button" id="Npd_UpdateData" class="btn btn-hgml" data-bs-toggle="modal" data-title="Save" onclick="ValidateNpdSaveandUpdate()"> Save and Update</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validatePmdReviewSendMailForm()" data-title="Send Mail"> Send Mail</button>
                    }
                    <button type="button" class="btn btn-cancel" data-bs-toggle="modal" data-bs-target="#CancelModal" data-title="Back">Back</button>
                </div>


            }
            @if (Model.CurrentStatusId == "11" && Model.Icon != "View")
            {
                <div class="button_top float-right mt-2 NotInNpdHgmlReview NotInHubReview NotInHgmlApprove NotInPmdReview">

                    @if (Model.IsEdit)
                    {
                        <button type="button" id="NPD_Save" class="btn btn-draft NotInApproved" data-bs-toggle="modal" data-title="Save" onclick="validateNpdSaveForm()">Save</button>
                        <button type="button" id="NPD_Submit" class="btn btn-hgml NotInApproved" data-bs-toggle="modal" data-title="Submit" onclick="ValidateSendToManagerForm()">Send For Approval</button>
                        <button type="button" id="SendToPmdSendback_" class="btn btn-hgml sendToPMDinDemoted SendToPmdSendback_" data-bs-toggle="modal" onclick="validateInitiatorsendtoPmd()" data-title="Send To PMD" style="display:none"> Send To PMD</button>
                        <button type="button" id="SendtoUnderExploration" class="btn btn-hgml SendtoUnderExploration SendtoUnderExplorationInSendBack" data-bs-toggle="modal" data-title="Submit" onclick="ValidatePackSubmitToExploration()" style="display:none">Send to PMD </button>
                    }
                    <button type="button" class="btn btn-cancel" data-bs-toggle="modal" data-bs-target="#CancelModal" data-title="Back">Back</button>
                </div>


            }

            @if (Model.CurrentStatusId == "5" || Model.CurrentStatusId == "16")
            {
                <div class="button_top float-right mt-2 NotInNpd NotInNpdHgmlReview NotInHubReview NotInHgmlApprove NotInApproved">

                    @if (Model.IsEdit)
                    {
                        <button type="button" class="btn btn-draft" data-bs-toggle="modal" onclick="validatePmdReviewSaveForm()" data-title="Save">Save</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validatePmdReviewSendBackFormToInitiator()" data-title="Send Back To Initiator">Send Back To Initiator</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validatePmdReviewSendBackForm()" data-title="Send Back">Send Back To HGML</button>
                        @if (Model.CurrentStatusId == "5")
                        {
                            <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validatePmdReviewToUnderExploration()" data-title="UnderExploration"> Send To Extended Fine Screening Review</button>
                        }
                        @if (Model.CurrentStatusId == "16")
                        {
                            <button type="button" class="btn btn-hgml" data-bs-toggle="modal" style="display:none" onclick="validatePmdReviewToUnderExploration()" data-title="Sent Back to Pmd"> Sent Back To PMD</button>
                        }
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validatePmdReviewAcceptForm()" data-title="Accept"> Accept</button>
                        <button type="button" class="btn btn-hgml" data-bs-toggle="modal" onclick="validatePmdReviewSendMailForm()" data-title="Send Mail"> Send Mail</button>
                    }
                    <button type="button" class="btn btn-hgml NotInRejected" data-bs-toggle="modal" onclick="validatePMDReviewRejectForm()" data-title="Reject">Reject</button>
                    <button type="button" class="btn btn-cancel" data-bs-toggle="modal" data-bs-target="#CancelModal" data-title="Back">Back</button>
                </div>

            }

        }

        @if (Model.Icon == "View")
        {
            <div class="button_top float-right mt-2">
                <button type="button" class="btn btn-cancel" data-bs-toggle="modal" data-bs-target="#CancelModal" data-title="Back">Back</button>
            </div>
        }
        <div>
            <ul class="link_href mb-2 mt-2">
                <li><a href="#ci" onclick="moveToTop('ci', 'highlight_section')" class="active">Project Details</a></li>
                <li><a href="#bh" onclick="moveToTop('bh', 'highlight_section')">Product Positioning</a></li>
                <li><a href="#sd" onclick="moveToTop('sd', 'highlight_section')">Product Profile: Formulation Profile</a></li>
                <li><a href="#tc" onclick="moveToTop('tc', 'highlight_section')">Product Profile: Packaging Profile</a></li>
                <li><a href="#ship_address" onclick="moveToTop('ship_address', 'highlight_section')">Sustainability</a></li>
                <li><a href="#ship_location" onclick="moveToTop('ship_location', 'highlight_section')">Business Information</a></li>
                <li><a href="#sd1" onclick="moveToTop('sd1', 'highlight_section')">Supporting Documents</a></li>
                @if (Model.CurrentStatusId == "1")
                {
                    <li><a href="#init" onclick="moveToTop('inti', 'highlight_section')">Initiator Remarks</a></li>
                }
                @if (Model.CurrentStatusId == "2")
                {
                    <li><a href="#init1" onclick="moveToTop('inti1', 'highlight_section')">Initiator Remarks</a></li>
                    <li><a href="#HGML1" onclick="moveToTop('HGML1', 'highlight_section')">HGML Data</a></li>
                }
                @if (Model.CurrentStatusId == "3")
                {
                    <li><a href="#HGMLhub" onclick="moveToTop('HGMLhub', 'highlight_section')">HGML / Hub Data</a></li>
                }
                @if (Model.CurrentStatusId == "4" || Model.CurrentStatusId == "14" || Model.CurrentStatusId == "5" || Model.CurrentStatusId == "16" || Model.CurrentStatusId == "6" || Model.CurrentStatusId == "12")
                {
                    <li><a href="#HGML1" onclick="moveToTop('HGML1', 'highlight_section')">HGML Data</a></li>
                }
                @if (Model.CurrentStatusId == "5" || Model.CurrentStatusId == "16" || Model.CurrentStatusId == "6" || Model.CurrentStatusId == "12")
                {
                    <li><a href="#finescreening" onclick="moveToTop('finescreening', 'highlight_section')">Fine Screening Data</a></li>
                }
            </ul>
        </div>
        <div class="cust_fields_master">
            <div id="ci" class="highlight_section ship_to">
                <div class="details_section">
                    <h4>Project Details</h4>
                    <div class="form_section">
                        <div class="form-group">
                            <label class="control-label">Project Name <span class="color-red">*</span></label>
                            @Html.TextBoxFor(model => model.ProjectName, new { @class = "form-control form-control-sm", @id = "Npd_ProjectName" })
                            <span style="color:red; display:none" id="Error_Npd_ProjectName" class="Error_ProjectDetails">Please enter Project Name</span>
                        </div>
                        <div class="form-group">
                            <label class="control-label"> Business Objective <span class="color-red">*</span></label>
                            @Html.TextAreaFor(model => model.BusinessObjective, new { @name = "editorpds", @class = "form-control", @id = "Npd_BusinessObjective" })
                            <span style="color:red; display:none" id="Error_Npd_BusinessObjective" class="Error_ProjectDetails">Please enter Business Objective </span>
                        </div>
                        <div class="add_grid_ NotInNpd NotInNpdHgmlReview NotInHubReview Project_Details_HubRemarks">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="">
                                        <div class="m-table__main mt-2 mb-4">
                                            <div class="m-table__responsive -virtual-scroll">
                                                <table id="Project_Details_Remarks" class="table table-bordered"></table>
                                                <div id="pager_worksheet1"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row NotInNpd">
                            <label class="col-sm-1 col-form-label">HGML Remarks</label>
                            <div class="col-sm-11 pd">
                                @Html.TextAreaFor(model => model.ProjectDetailsHGMLRemarks, new { @id = "PD_HGML_Remarks", @name = "editor2", @cols = "30", @class = "form-control form-control-sm" })
                            </div>
                        </div>
                        <div class="row mt-3 NotInNpd NotInNpdHgmlReview NotInHubReview NotInHgmlApprove OnlyPMDReject">
                            <label class="col-sm-1 col-form-label">Fine Screening Remarks</label>
                            <div class="col-sm-11 pd">
                                @Html.TextAreaFor(model => model.ProjectDetailsPmdRemarks, new { @id = "PD_Pmd_Remarks", @name = "editor2", @cols = "30", @class = "form-control form-control-sm" })

                            </div>
                        </div>
                        <div class="row mt-2 NotInNpd NotInNpdHgmlReview NotInHgmlApprove NotInPmdReview NotInApproved">
                            <label class="col-sm-1 col-form-label">@hub Remarks</label>
                            <div class="col-sm-11 pd">
                                @Html.TextAreaFor(model => model.ProjectDetailsHubRemarks, new { @id = "PD_HUB_Remarks", @name = "editor2", @cols = "30", @class = "form-control form-control-sm" })
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <div id="bh" class="ship_to">
                <div class="details_section">
                    <h4>Product Positioning</h4>
                    <span style="color:red;display:none" id="Error_ProductPositioning" class="Error_EmptyGrid">Please enter atleast one data for Product Positioning</span>
                    <div class="form_section">

                        <div class="form-group">
                            <label class="control-label">Target Consumer <span class="color-red">*</span></label>
                            @Html.TextBoxFor(model => model.ProductPositioningTargetConsumer, new { @class = "form-control form-control-sm", @id = "PP_TargetConsumer" })
                            <span style="color:red; display:none" id="Error_PP_TargetConsumer" class="Error_ProductPositioning">Please enter Target Consumer </span>
                        </div>
