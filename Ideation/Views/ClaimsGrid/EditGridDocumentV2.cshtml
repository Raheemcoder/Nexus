    ViewData["Title"] = "Edit Claims Grid";
    Layout = "/Views/Shared/CG_Layout.cshtml";
}
<style>
    ol.test_number {
        padding-left: 0px;
        list-style-type: decimal;
        position: relative;
        left: 20px;
    }
</style>
<link href="@Url.Content("~/NewAssets/assets/css/slick.min.css")" rel="stylesheet" />
<link href="@Url.Content("~/NewAssets/assets/css/slick-theme.min.css")" rel="stylesheet" />

<div id="">
    <section>
        <div class="-m_pagetitle">
            <div class="-m_title ">
                <h4><span>Master/</span> <span><a href="@Url.Content("~/ClaimsGrid/ClaimsGridDocument")">Claims Grid/</a></span> <label class="cen_txt">Edit Claims Grid</label></h4>
            </div>
        </div>

    </section>

    <style>
        .brd_tbl table td {
            border: 1px solid #339b9c !important;
        }

        .table tr td {
            overflow: hidden;
        }

        .formulation_table table thead th {
            font-size: 12px;
            font-weight: 400;
            border: 1px solid #000 !important;
            padding: 5px;
        }

        .d-flex {
            display: flex;
            justify-content: end;
        }

        .gap-10 {
            gap: 10px;
        }

        .packlabel_claims table tbody td, .communication_claims table tbody td, .packlabel_claims1 table tbody td, .communication_claims1 table tbody td {
            font-size: 12px;
            font-weight: 400;
            border: 1px solid #000 !important;
            vertical-align: baseline;
        }

        .packlabel_claims table thead tr, .communication_claims table thead tr, .packlabel_claims1 table thead tr, .communication_claims1 table thead tr {
            padding: 5px !important;
            background: #e2f3f0 !important;
        }

        .packlabel_claims table thead th, .communication_claims table thead th, .packlabel_claims1 table thead th, .communication_claims1 table thead th {
            border: none !important;
            padding: 5px !important;
        }

        .packlabel_claims table tbody td, .communication_claims table tbody td, .packlabel_claims1 table tbody td, .communication_claims1 table tbody td {
            font-size: 12px;
            font-weight: 400;
            border: 1px solid #000 !important;
            vertical-align: baseline;
            word-break: break-word;
        }

        .packlabel_claims table, .communication_claims table, .packlabel_claims1 table, .communication_claims1 table {
            margin-bottom: 10px
        }

        .packlabel_claims button, .communication_claims button, .packlabel_claims1 button, .communication_claims1 button {
            /*background: transparent;*/
            border: none;
        }

        .justify-center_1 {
            display: flex;
            float: right;
        }

        .remarks {
            display: flex;
            justify-content: space-between;
            padding-bottom: 5px;
        }

        .edit_icon_blue {
            display: flex;
            justify-content: center;
            align-items: center;
            padding: 6px;
            background: #1e78ff;
            border-radius: 50%;
            color: white;
            height: 25px;
        }

        .download_icon_green {
            display: flex;
            justify-content: center;
            align-items: center;
            padding: 6px;
            background: green;
            border-radius: 50%;
            color: white;
            height: 25px;
        }

        .delete_icon_red {
            display: flex;
            justify-content: center;
            align-items: center;
            padding: 6px;
            background: #c62828;
            border-radius: 50%;
            color: white;
            height: 25px;
        }

        .excelicon_row {
            display: flex;
            justify-content: end;
        }

        #DSG_Remarks textarea, #CFT_Remarks_Modal textarea {
            width: 100%;
            height: 100px;
            padding: 10px;
        }

        .claims_border-error {
            border-width: 1.5px;
            border-color: red;
            outline: none;
        }

        .color_red {
            color: red;
        }

        .addonpack {
            background: #01686a !important;
            color: #fff;
            padding: 2px 8px 2px 8px;
            border-top-left-radius: 10px;
            border-top-right-radius: 0px;
            border-bottom-left-radius: 0px;
            border-bottom-right-radius: 10px;
            border: 1px solid #01686a !important;
        }

        .paddingRight {
            padding-right: 5px !important;
        }

        .paddingLeft {
            padding-left: 5px !important;
        }

        .formulation_table table tbody td {
            width: 200px !important;
        }

        .download_icon_green i {
            color: white
        }

        .dark .communication_claims thead tr {
            background: #212529 !important;
        }
    </style>

    @if (TempData["Message"] != null)
    {
        <div class="alert @TempData["Messageclass"].ToString() alert-dismissable">
            <button type="button" class="btn-close status-alert" data-bs-dismiss="alert" aria-label="Close"></button>
            @TempData["Message"].ToString()
        </div>
    }

    <section>
        @* @if (Model.Stage != 3 && Model.Stage != 7 && ViewBag.isEdit == "edit")
